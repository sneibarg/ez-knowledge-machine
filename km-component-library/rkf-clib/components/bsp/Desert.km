(Desert has
   (superclasses (Terrain))
   (description ("A feature of the terrain")))

(every Desert has
   (trafficability ((a Trafficability-Value with
                       (value ((:pair *UR Armor-Unit)
                               (:pair *UR Infantry-Unit)))
   )))
)


