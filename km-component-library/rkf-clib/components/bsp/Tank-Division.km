(Tank-Division has 
   (superclasses (Tank-Unit Division)))

(every Tank-Division has-definition 
   (instance-of (Tank-Unit Division)))

(every Tank-Division has
   (element (
             (a Mechanized-Infantry-Brigade)
             (a Mechanized-Infantry-Brigade)
             (a Armored-Brigade)
             (a Armored-Brigade)
             (a Artillery-Brigade)
   ))
)


