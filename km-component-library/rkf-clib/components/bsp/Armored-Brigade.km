(Armored-Brigade has
   (superclasses (Armor-Unit Brigade)))

(every Armored-Brigade has-definition
   (instance-of (Armor-Unit Brigade)))

(every Armored-Brigade has
   (element (
             (a Armored-Battalion)
             (a Armored-Battalion)
             (a Direct-Support-Artillery-Battalion)
             (a Mechanized-Infantry-Battalion)
   ))
)

