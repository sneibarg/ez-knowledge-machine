(Fire has 
      (superclasses (Propel))
      (description ("Firing a projectile toward a target")))

(every Fire has
  (agent       ((a Military-Unit)))
  (object      ((a Tangible-Entity with
                   (plays ((a Projectile))))))
  (toward      ((a Spatial-Entity with
                   (plays ((a Target))))))
  (instrument  ((a Tangible-Entity with
                   (plays ((a Weapon))))))
)


