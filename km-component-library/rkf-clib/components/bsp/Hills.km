(Hills has
   (superclasses (Terrain))
   (description ("A feature of the terrain")))

(every Hills has
   (trafficability ((a Trafficability-Value with
                       (value ((:pair *R Armor-Unit)
                               (:pair *R Mechanized-Infantry-Unit)
                               (:pair *UR Light-Infantry-Unit)))
   )))
)


