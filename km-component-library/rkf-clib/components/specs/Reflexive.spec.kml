<kml>

<cliche name=Reflexive>

<hr>
<h3>Clich&eacute; Semantics:</h3>
<component tag=Reflexive>
  <definition>agent = object</definition>
  <requiredslot name=agent>
    <restriction>= <slot>object</slot></restriction>
  </requiredslot>
  <requiredslot name=object>
    <restriction>= <slot>agent</slot></restriction>
  </requiredslot>
</component>

</kml>


