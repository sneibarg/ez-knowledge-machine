(AgCl has (superclasses (Ionic-Compound)))
(every AgCl has
	(has-chemical-name ("silver chloride"))
	(has-part ((a Ag-Plus)(a Cl-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 Ag-Plus)
			(:pair 1 Cl-Minus)))))))
)


