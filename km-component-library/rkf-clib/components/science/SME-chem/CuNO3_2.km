(CuNO3_2 has (superclasses (Ionic-Compound)))
(every CuNO3_2 has
	(has-chemical-name ("copper(II) nitrate"))
	(has-part ((a Cu-Plus-2)(a NO3-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 Cu-Plus-2)
			(:pair 2 NO3-Minus)))))))
)


