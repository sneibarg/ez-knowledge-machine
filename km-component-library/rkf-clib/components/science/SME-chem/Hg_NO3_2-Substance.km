(Hg_NO3_2-Substance has (superclasses (Ionic-Compound-Substance)))
(every Hg_NO3_2-Substance has
        (has-chemical-name ("mercury nitrate"))
        (has-basic-structural-unit ((a Hg_NO3_2)))
)
(every Hg_NO3_2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a Hg_NO3_2))))

