(NaCl has (superclasses (Ionic-Compound)))

(every NaCl has (has-chemical-name ("sodium chloride")) (has-part ((a Na-Plus) (a Cl-Minus))) (has-chemical-formula ((a Chemical-Formula with (term ((:seq (:pair 1 Na-Plus) (:pair 1 Cl-Minus))))))))

(every NaCl has-definition (instance-of (Chemical-Entity)) (has-part ((a Na-Plus) (a Cl-Minus))))

