(Reaction-Na_Cl2--NaCl has (superclasses (Combination-Reaction)))

(every Reaction-Na_Cl2--NaCl has
(raw-material (
      (a Na-Substance)
      (a Molecule-Substance with (has-basic-structural-unit ((a Cl2))))
      ))
(result (
      (a NaCl-Substance))))
