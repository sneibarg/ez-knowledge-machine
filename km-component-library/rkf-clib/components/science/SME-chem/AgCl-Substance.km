(AgCl-Substance has (superclasses (Ionic-Compound-Substance)))
(every AgCl-Substance has
        (has-basic-structural-unit ((a AgCl)))
        (has-chemical-name ("silver chloride"))
)
(every AgCl-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a AgCl))))

