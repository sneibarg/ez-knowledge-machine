(Hg2I2-Substance has (superclasses (Ionic-Compound-Substance)))
(every Hg2I2-Substance has
        (has-chemical-name ("mercury(I) iodide"))
        (has-basic-structural-unit ((a Hg2I2)))
)
(every Hg2I2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a Hg2I2))))

