(CuNO3 has (superclasses (Ionic-Compound)))
(every CuNO3 has
	(has-chemical-name ("copper nitrate"))
	(has-part ((a Cu-Plus)(a NO3-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 Cu-Plus)
			(:pair 1 NO3-Minus)))))))
)


