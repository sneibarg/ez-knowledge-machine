(AuCl3 has (superclasses (Ionic-Compound)))
(every AuCl3 has
        (has-chemical-name ("gold chloride"))
        (has-part ((a Au-Plus-3)(a Cl-Minus)))
        (has-chemical-formula ((a Chemical-Formula with
                (term ((:seq
                        (:pair 1 Au-Plus-3)
                        (:pair 3 Cl-Minus)))))))
)
(every AuCl3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Au-Plus-3) (a Cl-Minus))))

