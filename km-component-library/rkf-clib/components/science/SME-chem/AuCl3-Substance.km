(AuCl3-Substance has (superclasses (Ionic-Compound-Substance)))
(every AuCl3-Substance has
        (has-basic-structural-unit ((a AuCl3)))
        (has-chemical-name ("gold chloride"))
)
(every AuCl3-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a AuCl3))))
