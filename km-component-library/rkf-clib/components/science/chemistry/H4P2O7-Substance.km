(H4P2O7-Substance has (superclasses (Acid)))
(every H4P2O7-Substance has
	(plays ((a Acid-Role with
		(default-ka ((a Equilibrium-Constant-Value with
			(value ((:pair 3.0e-2 nil))))))
		(intensity ((a Intensity-Value with
			(value ((:pair *weak Acid)))))))))
	(has-chemical-name ("Pyrophosphoric"))
	(has-basic-structural-unit ((a H4P2O7)))
)
