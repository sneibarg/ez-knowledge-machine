(KOH-Substance has 
  (superclasses (Base)))

(every KOH-Substance has
       (plays ((a Base-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Base)))))))))
	(has-basic-structural-unit 
	 ((a KOH)))
)

