(Cl has (superclasses (Non-Metal-Atom Halogen-Element)))
(every Cl has
       (has-molecular-name ("chloride"))
       (atomic-symbol (Cl))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 17 NIL))))))
       (chemical-period (3)))
