(H4IO6-Minus has (superclasses (Anion)))
(every H4IO6-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("tetrahydrogen orthoperiodate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 4 H)(:pair 1 I)(:pair 6 O)))))))
)
