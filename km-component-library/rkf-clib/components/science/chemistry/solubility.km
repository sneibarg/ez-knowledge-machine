(solubility has
   (instance-of (Property))
   (superslots 	())
   (subslots 	())
   (domain      (Chemical))    
   (range       (Solubility-Value))
   (scalar-constant-class (Solubility-Constant))
   (inverse 	(solubility-of))
   (fluent-status (*Inertial-Fluent))
   (inherit-with-overrides (t))
   (cardinality (N-to-1)))


