(I-Minus has (superclasses (Anion)))
(every I-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("iodide"))
	(has-acid-name ("hydroiodic"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 I)))))))
)
