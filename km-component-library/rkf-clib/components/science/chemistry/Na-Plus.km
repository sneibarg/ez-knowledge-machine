;; $Id: Na-Plus.km,v 1.8 2003/03/26 19:45:25 wilder Exp $ 
(Na-Plus has (superclasses (Metal-Cation)))
(every Na-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("sodium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 Na)))))))
)
