(O2-Minus-2 has (superclasses (Anion)))
(every O2-Minus-2 has
	(charge ((a Charge-Value with
		(value ((:pair -2 *electronic-charge))))))
	(has-chemical-name ("dioxide"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 2 O)))))))
)
