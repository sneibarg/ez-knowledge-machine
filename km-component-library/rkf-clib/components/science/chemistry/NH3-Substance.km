(NH3-Substance has (superclasses (Base)))
(every NH3-Substance has
       (plays ((a Base-Role with
		  (default-kb ((a Equilibrium-Constant-Value with
				  (value ((:pair 1.8e-5 nil))))))
		  (intensity ((a Intensity-Value with
			(value ((:pair *weak Base)))))))))
	(has-chemical-name ("Ammonia"))
	(has-basic-structural-unit ((a NH3)))
)
