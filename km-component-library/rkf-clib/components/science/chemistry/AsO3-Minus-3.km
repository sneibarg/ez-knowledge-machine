(AsO3-Minus-3 has (superclasses (Anion)))
(every AsO3-Minus-3 has
	(charge ((a Charge-Value with
		(value ((:pair -3 *electronic-charge))))))
	(has-chemical-name ("Arsenite"))
	(has-acid-name ("Arsenous"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 As)(:pair 3 O)))))))
)
