(Na2S has (superclasses (Ionic-Compound)))

(every Na2S has	
       (has-part 
	((a Na-Plus) (a S-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 2 Na-Plus)
			 (:pair 1 S-Minus-2)))))))
       )