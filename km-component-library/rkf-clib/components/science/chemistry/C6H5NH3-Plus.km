(C6H5NH3-Plus has (superclasses (Cation)))
(every C6H5NH3-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("anilinium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 6 C) 
						       (:pair 5 H)
						       (:pair 1 N)
						       (:pair 3 H)
						       ))))))
)
