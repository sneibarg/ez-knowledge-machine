(As-Plus-3 has (superclasses (Cation)))
(every As-Plus-3 has
	(charge ((a Charge-Value with
		(value ((:pair +3 *electronic-charge))))))
	(has-chemical-name ("arsenic(III)"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 As)))))))
)
