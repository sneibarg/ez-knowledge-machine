(K2CO3 has (superclasses (Ionic-Compound)))

(every K2CO3 has	
       (has-part 
	((a K2-Plus-2) (a CO3-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 K2-Plus-2)
		    (:pair 1 CO3-Minus-2))))))))
