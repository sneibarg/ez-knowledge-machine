(HC6H5O has (superclasses (Ionic-Compound)))
(every HC6H5O has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a C6H5O-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 C6H5O-Minus)))))))
)
