(HC7H5O2 has (superclasses (Ionic-Compound)))
(every HC7H5O2 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a C7H5O2-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 C7H5O2-Minus)))))))
)
