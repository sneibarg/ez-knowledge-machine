(PKa-Value has (superclasses (Property-Value)))

(every PKa-Value has
  (property-slot (pka))
       (less-than    ((must-be-a PKa-Value)))
       (greater-than ((must-be-a PKa-Value)))
       (same-as      ((must-be-a PKa-Value)))
)
