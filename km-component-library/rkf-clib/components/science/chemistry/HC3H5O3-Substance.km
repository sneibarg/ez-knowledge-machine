(HC3H5O3-Substance has (superclasses (Acid)))
(every HC3H5O3-Substance has
	(plays ((a Acid-Role with
		(default-ka ((a Equilibrium-Constant-Value with
			(value ((:pair 1.4e-4 nil))))))
		(intensity ((a Intensity-Value with
			(value ((:pair *weak Acid)))))))))
	(has-chemical-name ("Lactic"))
	(has-basic-structural-unit ((a HC3H5O3)))
)
