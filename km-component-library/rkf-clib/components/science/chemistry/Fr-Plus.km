(Fr-Plus has (superclasses (Metal-Cation)))

(every Fr-Plus has
       (charge ((a Charge-Value with
		   (value ((:pair 1 *electronic-charge))))))
       (has-chemical-formula ((a Chemical-Formula with
			     (term ((:seq (:pair 1 Fr))))))))
