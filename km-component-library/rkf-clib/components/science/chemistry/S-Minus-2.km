(S-Minus-2 has (superclasses (Anion)))
(every S-Minus-2 has
	(charge ((a Charge-Value with
		(value ((:pair -2 *electronic-charge))))))
	(has-chemical-name ("sulfide"))
	(has-acid-name ("hydrosulfuric"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 S)))))))
)
