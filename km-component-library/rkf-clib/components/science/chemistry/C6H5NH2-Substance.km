(C6H5NH2-Substance has (superclasses (Base)))
(every C6H5NH2-Substance has
	(plays ((a Base-Role with
		   (default-kb ((a Equilibrium-Constant-Value with	
				   (value ((:pair 4.3e-10 nil))))))	
		   (intensity ((a Intensity-Value with
				  (value ((:pair *weak Base)))))))))
	(has-chemical-name ("Aniline"))
	(has-basic-structural-unit ((a C6H5NH2)))
)
