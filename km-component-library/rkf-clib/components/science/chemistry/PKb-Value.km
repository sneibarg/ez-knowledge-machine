(PKb-Value has (superclasses (Property-Value)))

(every PKb-Value has
  (property-slot (pkb))
       (less-than    ((must-be-a PKb-Value)))
       (greater-than ((must-be-a PKb-Value)))
       (same-as      ((must-be-a PKb-Value)))
)
