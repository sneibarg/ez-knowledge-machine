(HONH2 has (superclasses (Ionic-Compound)))

(every HONH2 has
       (nested-atomic-chemical-formula (
				 (a Chemical-Formula with
				    (term ((:seq 
					    (:pair 1 H)
					    (:pair 1 O)
					    (:pair 1 N)
					    (:pair 2 H)
					    ))))))
       )
