(Mn-Plus-3 has (superclasses (Metal-Cation)))

(every Mn-Plus-3 has
       (has-chemical-formula ((a Chemical-Formula with
			     (term (
				    (:seq (:pair 1 Mn)))))))
       (atomic-chemical-formula ((a Chemical-Formula with
				    (term
				     ((:seq (:pair 1 Mn)))))))
       (charge ((a Charge-Value with
		   (value ((:pair 3 *electronic-charge)))))))
