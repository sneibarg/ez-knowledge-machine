(SO3-Minus-2 has (superclasses (Anion)))
(every SO3-Minus-2 has
	(charge ((a Charge-Value with
		(value ((:pair -2 *electronic-charge))))))
	(has-chemical-name ("sulfite"))
	(has-acid-name ("sulfurous"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 S) (:pair 3 O)))))))
)
