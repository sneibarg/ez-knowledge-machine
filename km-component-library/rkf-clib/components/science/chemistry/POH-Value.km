(POH-Value has (superclasses (Property-Value)))

(every POH-Value has
  (property-slot (pOH))
       (pOH-of 	     ((must-be-a Chemical-Entity)))
       (less-than    ((must-be-a POH-Value)))
       (greater-than ((must-be-a POH-Value)))
       (same-as      ((must-be-a POH-Value)))
)
