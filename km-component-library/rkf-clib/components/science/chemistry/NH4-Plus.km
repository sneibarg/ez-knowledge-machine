(NH4-Plus has (superclasses (Metal-Cation)))
(every NH4-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("ammonium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 N) 
						       (:pair 4 H)
						       ))))))
)
