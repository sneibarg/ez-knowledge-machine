(KI-Compound has 
  (superclasses (Ionic-Compound)))

(every KI-Compound has
       (has-part ((a K-Plus) (a I-Minus)))
       (has-chemical-formula (
         (a Chemical-Formula with
            (term ((:seq (:pair 1 K-Plus) (:pair 1 I-Minus)))))))
)
