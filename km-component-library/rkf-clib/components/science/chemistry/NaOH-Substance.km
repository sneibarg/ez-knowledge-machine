(NaOH-Substance has (superclasses (Base)))

(every NaOH-Substance has
       (plays ((a Base-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Base)))))))))
   (has-basic-structural-unit ((a NaOH)))
)

