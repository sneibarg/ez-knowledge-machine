(chemical-period has
    (instance-of (Property))
    (domain      (Atom))
    (range       (Number))
    (fluent-status (*Inertial-Fluent))
    (inherit-with-overrides (t))
    (inverse     (chemical-period-of))
    (cardinality (N-to-1))
)
