(NaOCl has (superclasses (Ionic-Compound)))

(every NaOCl has	
       (has-part 
	((a Na-Plus) (a OCl-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Na-Plus)
			 (:pair 1 OCl-Minus)))))))
       )