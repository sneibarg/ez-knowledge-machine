(HNO3-Substance has (superclasses (Acid)))

(every HNO3-Substance has
       (plays ((a Acid-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Acid)))))))))
       (has-basic-structural-unit((a HNO3)))
)