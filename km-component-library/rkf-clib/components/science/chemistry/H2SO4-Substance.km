(H2SO4-Substance has (superclasses (Acid)))
(every H2SO4-Substance has
	(plays ((a Acid-Role with
		(intensity ((a Intensity-Value with
			(value ((:pair *strong Acid)))))))))
	(has-chemical-name ("Sulfuric"))
	(has-basic-structural-unit ((a H2SO4)))
)
