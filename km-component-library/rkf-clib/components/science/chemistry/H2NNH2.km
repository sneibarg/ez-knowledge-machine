(H2NNH2 has (superclasses (Ionic-Compound)))

(every H2NNH2 has
       (nested-atomic-chemical-formula (
				 (a Chemical-Formula with
				    (term ((:seq 
					    (:pair 2 H)
					    (:pair 1 N)
					    (:pair 1 N)
					    (:pair 2 H)
					    ))))))
       )
