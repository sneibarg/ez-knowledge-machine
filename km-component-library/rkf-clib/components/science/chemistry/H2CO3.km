(H2CO3 has (superclasses (Ionic-Compound)))
(every H2CO3 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a CO3-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 CO3-Minus-2)))))))
)
