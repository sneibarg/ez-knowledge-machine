;; $Id: IO3-Minus.km,v 1.2 2003/02/22 16:25:05 jchaw Exp $ 
(IO3-Minus has (superclasses (Anion)))
(every IO3-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("iodate"))
	(has-acid-name ("iodic"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 I)(:pair 3 O)))))))
)
