(H3AsO4 has (superclasses (Ionic-Compound)))
(every H3AsO4 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a AsO4-Minus-3)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 3 H-Plus)
			(:pair 1 AsO4-Minus-3)))))))
)
