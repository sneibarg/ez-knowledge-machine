(HCl-Substance has (superclasses (Acid)))
(every HCl-Substance has
	(plays ((a Acid-Role with
		(intensity ((a Intensity-Value with
			(value ((:pair *strong Acid)))))))))
	(has-chemical-name ("Hydrochloric"))
	(has-basic-structural-unit ((a HCl)))
)
