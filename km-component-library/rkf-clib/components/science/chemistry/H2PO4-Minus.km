(H2PO4-Minus has (superclasses (Anion)))
(every H2PO4-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("dihydrogen phosphate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 2 H) (:pair 1 P) (:pair 4 O)))))))
)
