(S has (superclasses (Non-Metal-Atom VIA-Element)))
(every S has
       (has-molecular-name ("sulfide"))
       (atomic-symbol (S))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 16 NIL))))))
       (chemical-period (3)))
