(NaHCO3 has (superclasses (Ionic-Compound)))
(every NaHCO3 has
	(has-chemical-name (""))
	(has-part ((a Na-Plus)(a HCO3-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 Na-Plus)
			(:pair 1 HCO3-Minus)))))))
)
