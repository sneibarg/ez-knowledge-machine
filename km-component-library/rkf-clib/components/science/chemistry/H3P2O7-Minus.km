(H3P2O7-Minus has (superclasses (Anion)))
(every H3P2O7-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("trihydrogen pyrophosphate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 3 H)(:pair 2 P)(:pair 7 O)))))))
)
