(CNO-Minus has (superclasses (Anion)))
(every CNO-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("cyanate"))
	(has-acid-name ("cyanic"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 C)(:pair 1 N)(:pair 1 O)))))))
)
