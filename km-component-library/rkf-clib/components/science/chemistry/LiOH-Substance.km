(LiOH-Substance has (superclasses (Base)))

(every LiOH-Substance has
       (plays ((a Base-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Base)))))))))
       (has-basic-structural-unit ((a LiOH))))
