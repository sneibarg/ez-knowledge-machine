(Sn_NO3_2 has (superclasses (Ionic-Compound)))

(every Sn_NO3_2 has	
       (has-part 
	((a Sn-Plus-2) (a NO3-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Sn-Plus-2)
			 (:pair 2 NO3-Minus)))))))
)

