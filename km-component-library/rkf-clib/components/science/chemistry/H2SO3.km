(H2SO3 has (superclasses (Ionic-Compound)))
(every H2SO3 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a SO3-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 SO3-Minus-2)))))))
)
