(HClO3-Substance has (superclasses (Acid)))

(every HClO3-Substance has
       (plays ((a Acid-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Acid)))))))))
       (has-basic-structural-unit ((a HClO3))))
