(C2H5NH2 has (superclasses (Ionic-Compound)))

(every C2H5NH2 has
       (nested-atomic-chemical-formula (
				 (a Chemical-Formula with
				    (term ((:seq 
					    (:pair 2 C)
					    (:pair 5 H)
					    (:pair 1 N)
					    (:pair 2 H)
					    ))))))
       )
