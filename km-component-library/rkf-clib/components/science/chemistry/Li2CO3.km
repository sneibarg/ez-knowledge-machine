(Li2CO3 has (superclasses (Ionic-Compound)))

(every Li2CO3 has	
       (has-part 
	((a Li-Plus) (a CO3-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 2 Li-Plus)
			 (:pair 1 CO3-Minus-2)))))))
       )