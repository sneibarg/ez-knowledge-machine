(HC7H5O2-Substance has (superclasses (Acid)))
(every HC7H5O2-Substance has
	(plays ((a Acid-Role with
		(default-ka ((a Equilibrium-Constant-Value with
			(value ((:pair 6.5e-5 nil))))))
		(intensity ((a Intensity-Value with
			(value ((:pair *weak Acid)))))))))
	(has-chemical-name ("Benzoic"))
	(has-basic-structural-unit ((a HC7H5O2)))
)
