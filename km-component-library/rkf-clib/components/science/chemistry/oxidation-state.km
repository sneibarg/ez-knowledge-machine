(oxidation-state has
    (instance-of (Property))
    (domain      (Chemical-Entity))
    (range       (Oxidation-State-Value))
    (fluent-status (*Inertial-Fluent))
    (inherit-with-overrides (t))
    (inverse     (oxidation-state-of))
    (cardinality (N-to-1))
)
