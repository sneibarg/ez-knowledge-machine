(Be has (superclasses (Metal-Atom IIA-Element)))
(every Be has
       (atomic-symbol (Be))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 4 NIL))))))
       (atomic-weight ((a Atomic-Weight-Value with 
			  (value 
			   ((:pair 9.01 *dalton))))))
       (chemical-period (2)))
