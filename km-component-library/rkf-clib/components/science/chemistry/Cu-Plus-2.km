;; $Id: Cu-Plus-2.km,v 1.5 2003/03/26 19:45:23 wilder Exp $ 
(Cu-Plus-2 has (superclasses (Metal-Cation)))
(every Cu-Plus-2 has
	(charge ((a Charge-Value with
		(value ((:pair +2 *electronic-charge))))))
	(has-chemical-name ("copper(II)"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 Cu)))))))
)
