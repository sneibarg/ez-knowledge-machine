(V-Plus has (superclasses (Metal-Cation)))
(every V-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("vanadium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 V)))))))
)
