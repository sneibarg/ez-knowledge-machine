(Ra-Plus-2 has (superclasses (Metal-Cation)))

(every Ra-Plus-2 has
       (has-chemical-formula ((a Chemical-Formula with
			     (term (
				    (:seq (:pair 1 Ra)))))))
       (charge ((a Charge-Value with
		   (value ((:pair 2 *electronic-charge)))))))
