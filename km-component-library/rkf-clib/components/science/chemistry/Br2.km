(Br2 has (superclasses (Molecular-Compound)))

(every Br2 has	
       (nested-atomic-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 2 Br)))))))
       (has-chemical-formula
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 2 Br)))))))
       )
