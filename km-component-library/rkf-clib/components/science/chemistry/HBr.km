(HBr has (superclasses (Ionic-Compound)))

(every HBr has
#|       (nested-atomic-chemical-formula (
					(a Chemical-Formula with
					   (term ((:seq 
						   (:pair 1 H)
						   (:pair 1 Br)))))))
|#
       (has-chemical-formula (
			      (a Chemical-Formula with
				 (term ((:seq
					 (:pair 1 H-Plus)
					 (:pair 1 Br-Minus)))))))
       )
