(HC3H5O3 has (superclasses (Ionic-Compound)))
(every HC3H5O3 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a C3H5O3-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 C3H5O3-Minus)))))))
)
