(HSO4-Minus-Substance has (superclasses (Anion-Substance)))
(every HSO4-Minus-Substance has
	(plays ((a Acid-Role with
		(default-ka ((a Equilibrium-Constant-Value with
			(value ((:pair 1.2e-2 nil))))))
		(intensity ((a Intensity-Value with
			(value ((:pair *weak Acid)))))))))
	(has-chemical-name (""))
	(has-basic-structural-unit ((a HSO4-Minus)))
)
