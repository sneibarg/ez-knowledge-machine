(NH4Cl has (superclasses (Salt)))

(every NH4Cl has
       (has-part ((a NH4-Plus)
		  (a Cl-Minus)))
       
       (has-chemical-formula ((a Chemical-Formula with
				 (term ((:seq (:pair 1 NH4-Plus)
					      (:pair 1 Cl-Minus))))))
			     )
       (nested-atomic-chemical-formula ((a Chemical-Formula with
				 (term ((:seq (:pair 1 N)
					      (:pair 4 H)
					      (:pair 1 Cl))))))
			     )
       )
