(H3O-Plus has (superclasses (Cation)))
(every H3O-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("hydronium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 3 H) (:pair 1 O)))))))
)
