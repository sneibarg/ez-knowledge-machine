(NaOH has (superclasses (Ionic-Compound)))

(every NaOH has	
       (has-part 
	((a Na-Plus) (a OH-Minus)))
       (atomic-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Na)
			 (:pair 1 O)
			 (:pair 1 H)))))))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Na-Plus)
			 (:pair 1 OH-Minus)))))))
       )