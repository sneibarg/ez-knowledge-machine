(CH3_2_NH2-Plus has (superclasses (Cation)))
(every CH3_2_NH2-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("Dimethlyammonium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq 
						  (:pair 2 
							 (:seq
							  (:pair 1 C)
							  (:pair 3 H)))
						  (:pair 1 N)
						  (:pair 2 H)
						       ))))))
)
