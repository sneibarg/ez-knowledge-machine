(HSO4-Minus has (superclasses (Anion)))
(every HSO4-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("hydrogen sulfate" "bisulfate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 H) (:pair 1 S) (:pair 4 O)))))))
)
