;; $Id: Mn-Plus-2.km,v 1.6 2003/03/26 19:45:25 wilder Exp $ 
(Mn-Plus-2 has (superclasses (Metal-Cation)))
(every Mn-Plus-2 has
	(charge ((a Charge-Value with
		(value ((:pair +2 *electronic-charge))))))
	(has-chemical-name ("manganese"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 Mn)))))))
)
