(BO3-Minus-3 has (superclasses (Anion)))
(every BO3-Minus-3 has
	(charge ((a Charge-Value with
		(value ((:pair -3 *electronic-charge))))))
	(has-chemical-name ("Borate"))
	(has-acid-name ("Boric"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 B)(:pair 3 O)))))))
)
