(H2P2O7-Minus-2 has (superclasses (Anion)))
(every H2P2O7-Minus-2 has
	(charge ((a Charge-Value with
		(value ((:pair -2 *electronic-charge))))))
	(has-chemical-name ("dihydrogen pyrophosphate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 2 H)(:pair 2 P)(:pair 7 O)))))))
)
