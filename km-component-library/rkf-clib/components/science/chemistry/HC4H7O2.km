(HC4H7O2 has (superclasses (Ionic-Compound)))
(every HC4H7O2 has
	(has-chemical-name (""))
	(has-part ((a H-Plus)(a C4H7O2-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 C4H7O2-Minus)))))))
)
