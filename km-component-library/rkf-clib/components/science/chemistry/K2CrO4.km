(K2CrO4 has (superclasses (Ionic-Compound)))

(every K2CrO4 has	
       (has-part 
	((a K2-Plus-2) (a CrO4-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 K2-Plus-2)
			 (:pair 1 CrO4-Minus-2))))))))
