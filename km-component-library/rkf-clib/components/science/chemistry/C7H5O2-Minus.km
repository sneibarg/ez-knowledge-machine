(C7H5O2-Minus has (superclasses (Anion)))
(every C7H5O2-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *electronic-charge))))))
	(has-chemical-name ("benzoate"))
	(has-acid-name ("benzoic"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 7 C)(:pair 5 H)(:pair 2 O)))))))
)
