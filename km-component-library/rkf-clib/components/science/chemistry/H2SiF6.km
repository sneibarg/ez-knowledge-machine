(H2SiF6 has (superclasses (Ionic-Compound)))

(every H2SiF6 has
       (has-part ((a H-Plus)(a SiF6-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 2 H-Plus)
		    (:pair 1 SiF6-Minus-2))))))))