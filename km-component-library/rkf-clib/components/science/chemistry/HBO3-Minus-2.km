(HBO3-Minus-2 has (superclasses (Anion)))
(every HBO3-Minus-2 has
	(charge ((a Charge-Value with
		(value ((:pair -2 *electronic-charge))))))
	(has-chemical-name ("hydrogen borate"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 H) (:pair 1 B) (:pair 3 O)))))))
)
