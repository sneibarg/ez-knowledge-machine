;; $Id: Mg-Plus-2.km,v 1.4 2003/03/26 19:45:25 wilder Exp $ 
(Mg-Plus-2 has (superclasses (Metal-Cation)))
(every Mg-Plus-2 has
	(charge ((a Charge-Value with
		(value ((:pair +2 *electronic-charge))))))
	(has-chemical-name ("magnesium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 Mg)))))))
)
