(Fe-Plus-2 has (superclasses (Metal-Cation)))
(every Fe-Plus-2 has
	(charge ((a Charge-Value with
		(value ((:pair +2 *electronic-charge))))))
	(has-chemical-name ("iron(II)"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 Fe)))))))
)
