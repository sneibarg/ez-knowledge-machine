(HI-Substance has (superclasses (Acid)))

(every HI-Substance has
       (plays ((a Acid-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Acid)))))))))
       (has-basic-structural-unit ((a HI))))
