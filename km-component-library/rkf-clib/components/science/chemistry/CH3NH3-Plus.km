;; $Id: CH3NH3-Plus.km,v 1.3 2003/03/27 17:26:10 wilder Exp $ 
(CH3NH3-Plus has (superclasses (Metal-Cation)))
(every CH3NH3-Plus has
	(charge ((a Charge-Value with
		(value ((:pair +1 *electronic-charge))))))
	(has-chemical-name ("Methylammonium"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 C) 
						       (:pair 3 H)
						       (:pair 1 N)
						       (:pair 3 H)
						       ))))))
)
