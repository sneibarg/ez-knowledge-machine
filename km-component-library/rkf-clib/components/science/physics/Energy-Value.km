(Energy-Value has 
		(superclasses (Property-Value)))

(every Energy-Value has
  (property-slot (energy))
       (energy-of ((must-be-a Entity)))
       (less-than ((must-be-a Energy-Value)))
       (greater-than ((must-be-a Energy-Value)))
       (same-as ((must-be-a Energy-Value)))
)
