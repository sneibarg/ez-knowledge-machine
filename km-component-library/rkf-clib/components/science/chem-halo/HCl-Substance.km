(HCl-Substance has (superclasses (Bronsted-Lowry-Acid)))
(every HCl-Substance has
	(has-chemical-name ("hydrochloric"))
	(has-basic-structural-unit ((a HCl)))
)
(every HCl-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a HCl))))
