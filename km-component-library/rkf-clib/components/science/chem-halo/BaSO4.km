(BaSO4 has (superclasses (Ionic-Compound)))

(every BaSO4 has	
       (has-chemical-name ("barium sulfate"))
       (has-part 
	((a Ba-Plus-2) (a SO4-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Ba-Plus-2)
			 (:pair 1 SO4-Minus-2)))))))
       )(every BaSO4 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Ba-Plus-2) (a SO4-Minus-2))))

