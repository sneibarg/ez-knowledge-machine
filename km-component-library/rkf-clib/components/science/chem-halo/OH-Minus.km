(OH-Minus has (superclasses (Anion)))
(every OH-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *unity))))))
	(has-chemical-name ("hydroxide"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 O) (:pair 1 H)))))))
)


(every OH-Minus has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("hydroxide"))
)

(OH-Minus has (wn20-synset ((:triple "OH-" 0 "n"))))
