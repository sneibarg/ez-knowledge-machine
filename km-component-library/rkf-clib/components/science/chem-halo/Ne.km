(Ne has (superclasses (Non-Metal-Atom VIIIA-Noble-Gas-Element)))
(every Ne has
       (atomic-symbol (Ne))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 10 NIL))))))
       (chemical-period (2)))
(every Ne has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("neon"))
)
