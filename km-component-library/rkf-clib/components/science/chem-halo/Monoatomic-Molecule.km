(Monoatomic-Molecule has
		     (superclasses (Molecule)))


(Monoatomic-Molecule has
		     (user-description ("A molecule that contains only one atom.")))

(every Monoatomic-Molecule has-definition
       (instance-of (Molecule))
       (has-part ((exactly 1 Atom))))

(Monoatomic-Molecule has (wn20-synset ((:set
					(:triple "monoatomic molecule" 0 "n")))))

