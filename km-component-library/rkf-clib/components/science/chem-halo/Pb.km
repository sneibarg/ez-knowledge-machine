(Pb has (superclasses (Metal-Atom IVA-Element)))
(every Pb has
       (atomic-symbol (Pb))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 82 NIL))))))
       (has-ion ((a Pb-Plus-2)))
       (chemical-period (6)))

(every Pb has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("lead"))
)
