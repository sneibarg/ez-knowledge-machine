(Cl has (superclasses (Non-Metal-Atom VIIA-Halogen-Element)))
(every Cl has
       (has-molecular-name ("chloride"))
       (atomic-symbol (Cl))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 17 NIL))))))
       (has-ion ((a Cl-Minus)))
       (chemical-period (3)))

(every Cl has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("chlorine"))
)
