(Mg has (superclasses (Metal-Atom IIA-Alkaline-Earth-Metal-Element)))
(every Mg has
       (atomic-symbol (Mg))
       (has-ion ((a Mg-Plus-2)))
       (chemical-period (3)))

(every Mg has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("magnesium"))
)
