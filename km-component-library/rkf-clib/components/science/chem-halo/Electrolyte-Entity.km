(Electrolyte-Entity has
  (superclasses (Chemical-Entity)))

(Electrolyte-Entity has
  (user-description ("An electrolyte is a solute that produces ions in solution; an electrolytic solution conducts an electric current.")))

#|

(every Electrolyte-Entity has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Anion) (a Cation))))
|#
