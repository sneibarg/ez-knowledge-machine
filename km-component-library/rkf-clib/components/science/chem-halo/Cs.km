(Cs has (superclasses (Metal-Atom IA-Alkali-Metal-Element)))

(every Cs has
       (atomic-symbol (Cs))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 55 NIL))))))
       (has-ion ((a Cs-Plus)))
       (chemical-period (6)))

(every Cs has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("cesium"))
)
