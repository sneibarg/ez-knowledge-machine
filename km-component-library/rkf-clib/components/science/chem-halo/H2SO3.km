(H2SO3 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H2SO3 has
	(has-chemical-name ("sulfurous acid"))
	(has-part ((a H-Plus)(a SO3-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 SO3-Minus-2)))))))
)
(every H2SO3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a SO3-Minus-2))))

