(NaNO3-Substance has 
  (superclasses (Ionic-Compound-Substance)))

(every NaNO3-Substance has
  (has-basic-structural-unit (
    (a NaNO3)))
)

(every NaNO3-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a NaNO3))))
