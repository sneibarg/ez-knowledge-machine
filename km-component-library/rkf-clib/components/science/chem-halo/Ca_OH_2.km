(Ca_OH_2 has (superclasses (Ionic-Compound)))

(every Ca_OH_2 has	
       (has-chemical-name ("calcium hydroxide"))
       (has-part 
	((a Ca-Plus-2) (a OH-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Ca-Plus-2)
			 (:pair 2 OH-Minus)))))))
)(every Ca_OH_2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Ca-Plus-2) (a OH-Minus))))

