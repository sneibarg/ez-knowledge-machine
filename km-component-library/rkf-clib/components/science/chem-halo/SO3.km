(SO3 has (superclasses (Inorganic-Molecule)))

(every SO3 has	
       (has-chemical-name ("sulfur trioxide"))
       (nested-atomic-chemical-formula
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 S)
		    (:pair 3 O))))))))

(every SO3 has-definition
  (instance-of (Chemical-Entity))
  (has-chemical-name ("sulfur trioxide"))
)
