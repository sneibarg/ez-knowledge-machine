(Pb_OH_2 has (superclasses (Ionic-Compound)))

(every Pb_OH_2 has	
       (has-chemical-name ("lead hydroxide"))
       (has-part 
	((a Pb-Plus-2) (a OH-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Pb-Plus-2)
			 (:pair 2 OH-Minus)))))))
)(every Pb_OH_2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Pb-Plus-2) (a OH-Minus))))

