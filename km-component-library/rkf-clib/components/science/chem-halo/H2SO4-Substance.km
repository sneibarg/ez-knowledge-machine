(H2SO4-Substance has (superclasses (Bronsted-Lowry-Acid)))
(every H2SO4-Substance has
	(has-chemical-name ("sulfuric"))
	(has-basic-structural-unit ((a H2SO4)))
)
(every H2SO4-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a H2SO4))))
