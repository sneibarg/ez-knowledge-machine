(Ag has (superclasses (Metal-Atom IB-Element)))
(every Ag has
       (atomic-symbol (Ag))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 47 NIL))))))
       (has-ion ((a Ag-Plus)))
       (chemical-period (5)))

(every Ag has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("silver"))
)
