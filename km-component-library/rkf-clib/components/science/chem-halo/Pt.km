(Pt has (superclasses (Metal-Atom VIIIB-Element)))
(every Pt has
       (atomic-symbol (Pt))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 78 NIL))))))
       (has-ion ((a Pt-Plus-2)))
       (chemical-period (6)))


(every Pt has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("platinum"))
)
