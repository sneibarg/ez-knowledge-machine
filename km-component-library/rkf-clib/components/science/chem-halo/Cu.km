(Cu has (superclasses (Metal-Atom IB-Element)))
(every Cu has
       (atomic-symbol (Cu))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 29 NIL))))))
       (has-ion ((a Cu-Plus-2)))
       (chemical-period (4)))

(every Cu has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("copper"))
)
