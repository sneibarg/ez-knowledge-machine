(H2SiF6-Substance has (superclasses(Bronsted-Lowry-Acid)))

(every H2SiF6-Substance has
       (plays ((a Acid-Role with
		  (intensity ((a Intensity-Value with
				 (value ((:pair *strong Acid)))))))))
       (has-basic-structural-unit ((a H2SiF6))))


(every H2SiF6-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a H2SiF6))))
