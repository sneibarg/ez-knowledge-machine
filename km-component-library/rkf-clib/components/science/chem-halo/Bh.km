(Bh has (superclasses (Metal-Atom VIIB-Element)))
(every Bh has
       (atomic-symbol (Bh))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 107 NIL))))))
       (chemical-period (7)))

(every Bh has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("bohrium"))
)
