(Li has (superclasses (Metal-Atom IA-Alkali-Metal-Element)))
(every Li has
       (atomic-symbol (Li))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 3 NIL))))))
       (has-ion ((a Li-Plus)))
       (chemical-period (2)))

(every Li has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("lithium"))
)
