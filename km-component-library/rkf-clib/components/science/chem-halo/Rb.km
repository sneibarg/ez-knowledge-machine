(Rb has (superclasses (Metal-Atom IA-Alkali-Metal-Element)))
(every Rb has
       (atomic-symbol (Rb))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 39 NIL))))))
       (has-ion ((a Rb-Plus)))
       (chemical-period (5)))
(every Rb has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("rubidium"))
)
