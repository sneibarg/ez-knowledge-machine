(H-Minus has (superclasses (Anion)))
(every H-Minus has
	(charge ((a Charge-Value with
		(value ((:pair -1 *unity))))))
	(has-chemical-name ("hydride"))
	(nested-atomic-chemical-formula
		((a Chemical-Formula with (term ((:seq (:pair 1 H)))))))
        (is-ion-of ((a H)))
)

(every H-Minus has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("hydride"))
)


(H-Minus has (wn20-synset ((:triple "H-" 0 "n"))))