(CO-Compound-Substance has (superclasses (Inorganic-Molecule-Substance)))

(every CO-Compound-Substance has
       (has-basic-structural-unit ((a CO-Compound)))
       (physical-state ((a State-Value with
		  (value (*gas))))))
(every CO-Compound-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a CO-Compound))))
