(H2 has (superclasses (Non-Metal-Molecule)))

(every H2 has	
       (has-chemical-name ("hydrogen gas"))
       (nested-atomic-chemical-formula (
			  (a Chemical-Formula with
			     (term ((:seq
				     (:pair 2 H)))))))
       (has-part ((a H)))
)


(every H2 has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("hydrogen gas"))
)
