(NaNO3 has (superclasses (Ionic-Compound)))

(every NaNO3 has
       (has-chemical-name ("sodium nitrate"))
       (has-part ((a Na-Plus)
		  (a NO3-Minus)))

       (has-chemical-formula ((a Chemical-Formula with
				 (term ((:seq (:pair 1 Na-Plus)
					      (:pair 1 NO3-Minus))))))
			     )
       )
(every NaNO3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Na-Plus) (a NO3-Minus))))

