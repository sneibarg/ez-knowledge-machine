(P has (superclasses (Non-Metal-Atom VA-Element)))
(every P has
       (has-molecular-name ("phosphide"))
       (atomic-symbol (P))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 15 NIL))))))
       (chemical-period (3)))

(every P has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("phosphorus"))
)
