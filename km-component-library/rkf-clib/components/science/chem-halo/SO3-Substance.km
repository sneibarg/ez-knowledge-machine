(SO3-Substance has (superclasses (Inorganic-Molecule-Substance)))

(every SO3-Substance has
       (has-basic-structural-unit ((a SO3))))

(every SO3-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a SO3))))
