(Mn has (superclasses (Metal-Atom VIIB-Element)))
(every Mn has
       (atomic-symbol (Mn))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 25 NIL))))))
       (has-ion ((a Mn-Plus-2)))
       (chemical-period (4)))

(every Mn has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("manganese"))
)
