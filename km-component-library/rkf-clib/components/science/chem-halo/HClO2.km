(HClO2 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every HClO2 has
	(has-chemical-name ("hydrogen chlorite"))
	(has-part ((a H-Plus)(a ClO2-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 ClO2-Minus)))))))
)
(every HClO2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a ClO2-Minus))))

