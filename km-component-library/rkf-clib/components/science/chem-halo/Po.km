(Po has (superclasses (Metal-Atom VIA-Chalcogen-Element)))
(every Po has
       (atomic-symbol (Po))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 84 NIL))))))
       (chemical-period (6)))
(every Po has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("polonium"))
)
