(Ba_OH_2 has (superclasses (Ionic-Compound)))

(every Ba_OH_2 has	
       (has-chemical-name ("barium hydroxide"))
       (has-part ((a Ba-Plus-2) (a OH-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with 
	    (term ((:seq
		    (:pair 1 Ba-Plus-2)
		    (:pair 2 OH-Minus))))))))


(every Ba_OH_2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Ba-Plus-2) (a OH-Minus))))

