(Rh has (superclasses (Metal-Atom VIIIB-Element)))
(every Rh has
       (atomic-symbol (Rh))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 45 NIL))))))
       (chemical-period (5)))


(every Rh has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("rhodium"))
)
