(C3H7OH-Substance has (superclasses (Organic-Molecule-Substance)))
	
(every C3H7OH-Substance has
       (has-chemical-name ("propanol"))	
       (has-basic-structural-unit ((a C3H7OH))))
(every C3H7OH-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a C3H7OH))))
