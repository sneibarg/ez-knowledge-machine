(Non-Metal-Molecule has 
	   (superclasses (Inorganic-Molecule)))

(every Non-Metal-Molecule has
       (has-part ((a Non-Metal-Atom))))

(Non-Metal-Molecule has 
  (user-description ("Nonmetal molecules are formed by nonmetal elements in the upper right corner of the periodic table; nonmetals differ from metals in their physical and chemical properties.")))

