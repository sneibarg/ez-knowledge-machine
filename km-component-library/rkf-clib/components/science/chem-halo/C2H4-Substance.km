(C2H4-Substance has (superclasses (Hydrocarbon-Substance)))
	
(every C2H4-Substance has
       (has-chemical-name ("ethlene" "ethene"))	
       (has-basic-structural-unit ((a C2H4))))
(every C2H4-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a C2H4))))
