(PbO has (superclasses (Ionic-Compound)))

(every PbO has	
       (has-chemical-name ("lead oxide"))
       (has-part 
	((a Pb-Plus-2) (a O-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Pb-Plus-2)
			 (:pair 1 O-Minus-2)))))))
)
(every PbO has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Pb-Plus-2) (a O-Minus-2))))

