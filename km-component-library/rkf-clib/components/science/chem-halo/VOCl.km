(VOCl has (superclasses (Inorganic-Molecule)))

(every VOCl has
       (has-chemical-name ("vanadium hypochlorite"))
       (nested-atomic-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 V)
			 (:pair 1 O)
			 (:pair 1 Cl)))))))
)


(every VOCl has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("vanadium hypochlorite"))
)
