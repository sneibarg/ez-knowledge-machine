(In has (superclasses (Metal-Atom IIIA-Element)))
(every In has
       (atomic-symbol (In))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 49 NIL))))))
       (chemical-period (5)))
(every In has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("indium"))
)
