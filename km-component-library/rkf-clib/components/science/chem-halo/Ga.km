(Ga has (superclasses (Metal-Atom IIIA-Element)))
(every Ga has
       (atomic-symbol (Ga))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 31 NIL))))))
       (chemical-period (4)))

(every Ga has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("gallium"))
)
