(Fr has (superclasses (Metal-Atom IA-Alkali-Metal-Element)))
(every Fr has
       (atomic-symbol (Fr))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 87 NIL))))))
       (has-ion ((a Fr-Plus)))
       (chemical-period (7)))
(every Fr has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("francium"))
)
