(As has (superclasses (Metalloid-Atom VA-Element)))
(every As has
       (atomic-symbol (As))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 33 NIL))))))
       (has-ion ((a As-Plus-3)))
       (chemical-period (4)))
(every As has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("arsenic"))
)
