(Zr has (superclasses (Metal-Atom IVB-Element)))
(every Zr has
       (atomic-symbol (Zr))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 40 NIL))))))
       (chemical-period (5)))
(every Zr has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("zirconium"))
)
