(Si has (superclasses (Metalloid-Atom IVA-Element)))
(every Si has
       (atomic-symbol (Si))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 14 NIL))))))
       (chemical-period (3)))
(every Si has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("silicon"))
)
