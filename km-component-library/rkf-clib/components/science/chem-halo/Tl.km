(Tl has (superclasses (Metal-Atom IIIA-Element)))
(every Tl has
       (atomic-symbol (Tl))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 81 NIL))))))
       (chemical-period (6)))
(every Tl has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("thallium"))
)
