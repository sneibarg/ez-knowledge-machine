(CH3COOH has (superclasses (Ionic-Compound)))
(every CH3COOH has
       (has-chemical-name ("hydrogen acetate" "acetic acid" "ethanoic acid"))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 H-Plus)
		    (:pair 1 CH3COO-Minus))))))))
(every CH3COOH has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a CH3COO-Minus) (a H-Plus))))

