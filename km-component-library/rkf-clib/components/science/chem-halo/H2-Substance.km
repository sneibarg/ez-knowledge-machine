(H2-Substance has (superclasses (Non-Metal)))
	
(every H2-Substance has
  (has-basic-structural-unit ((a H2))))
(every H2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a H2))))

(H2-Substance has (wn20-synset ((:set
				 (:triple "hydrogen" 0 "n")))))