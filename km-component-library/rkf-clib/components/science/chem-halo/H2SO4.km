(H2SO4 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H2SO4 has
	(has-chemical-name ("sulfuric acid" "hydrogen sulfate"))
	(has-part ((a H-Plus)(a SO4-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 SO4-Minus-2)))))))
)
(every H2SO4 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a SO4-Minus-2))))

