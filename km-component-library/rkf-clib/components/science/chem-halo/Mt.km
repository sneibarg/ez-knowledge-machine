(Mt has (superclasses (Metal-Atom VIIIB-Element)))
(every Mt has
       (atomic-symbol (Mt))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 109 NIL))))))
       (chemical-period (7)))
(every Mt has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("meitnerium"))
)
