(H2C4H4O6 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H2C4H4O6 has
	(has-chemical-name ("succinic acid" "butanedioc acid"))
	(has-part ((a H-Plus)(a C4H4O6-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 C4H4O6-Minus-2)))))))
)
(every H2C4H4O6 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a C4H4O6-Minus-2))))

