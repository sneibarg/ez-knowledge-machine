(K has (superclasses (Metal-Atom IA-Alkali-Metal-Element)))
(every K has
       (atomic-symbol (K))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 19 NIL))))))
       (has-ion ((a K-Plus)))
       (chemical-period (4)))

(every K has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("potassium"))
)
