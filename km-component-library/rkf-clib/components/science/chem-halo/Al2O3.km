(Al2O3 has (superclasses (Ionic-Compound)))

(every Al2O3 has	
       (has-chemical-name ("aluminum oxide"))
       (has-part 
	((a Al-Plus-3) (a O-Minus-2)))
       (has-chemical-formula 
	((a Chemical-Formula with
            (term ((:seq
		    (:pair 2 Al-Plus-3)
		    (:pair 3 O-Minus-2))))))))

(every Al2O3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Al-Plus-3) (a O-Minus-2))))

