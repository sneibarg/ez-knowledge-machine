(Ru has (superclasses (Metal-Atom VIIIB-Element)))
(every Ru has
       (atomic-symbol (Ru))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 44 NIL))))))
       (chemical-period (5)))

(every Ru has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("ruthenium"))
)
