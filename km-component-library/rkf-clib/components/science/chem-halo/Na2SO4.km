(Na2SO4 has (superclasses (Ionic-Compound)))
(every Na2SO4 has
	(has-chemical-name ("sodium sulfate"))
	(has-part ((a Na-Plus)(a SO4-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 Na-Plus)
			(:pair 1 SO4-Minus-2)))))))
)
(every Na2SO4 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Na-Plus) (a SO4-Minus-2))))

