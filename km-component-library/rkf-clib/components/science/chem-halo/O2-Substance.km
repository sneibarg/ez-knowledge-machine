(O2-Substance has (superclasses (Non-Metal)))
	
(every O2-Substance has
  (has-basic-structural-unit ((a O2))))
(every O2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a O2))))

(O2-Substance has (wn20-synset ((:set
				 (:triple "oxygen" 0 "n")))))
