(C2H6-Substance has (superclasses (Hydrocarbon-Substance)))
	
(every C2H6-Substance has
       (has-chemical-name ("ethane"))	
       (has-basic-structural-unit ((a C2H6))))
(every C2H6-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a C2H6))))
