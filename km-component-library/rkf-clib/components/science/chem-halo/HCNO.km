(HCNO has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every HCNO has
	(has-chemical-name ("cyanic acid"))
	(has-part ((a H-Plus)(a CNO-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 CNO-Minus)))))))
)
(every HCNO has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a CNO-Minus))))

