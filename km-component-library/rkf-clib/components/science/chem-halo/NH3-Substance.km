(NH3-Substance has (superclasses (Inorganic-Molecule-Substance)))
(every NH3-Substance has
	(has-chemical-name ("ammonia"))
	(has-basic-structural-unit ((a NH3)))
)
(every NH3-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a NH3))))
