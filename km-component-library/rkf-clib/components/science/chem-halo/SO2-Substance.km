(SO2-Substance has (superclasses (Inorganic-Molecule-Substance)))

(every SO2-Substance has
       (has-basic-structural-unit ((a SO2)))
       (physical-state ((a State-Value with
		  (value (*gas))))))
(every SO2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a SO2))))
