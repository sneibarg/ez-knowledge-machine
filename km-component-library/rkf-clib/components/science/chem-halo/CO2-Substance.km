(CO2-Substance has (superclasses (Inorganic-Molecule-Substance)))

(every CO2-Substance has
       (has-basic-structural-unit ((a CO2)))
       (physical-state ((a State-Value with
		  (value (*gas))))))
(every CO2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a CO2))))
