(Sc has (superclasses (Metal-Atom IIIB-Element)))
(every Sc has
       (atomic-symbol (Sc))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 21 NIL))))))
       (chemical-period (4)))

(every Sc has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("scandium"))
)
