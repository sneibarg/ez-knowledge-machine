(CsOH has (superclasses (Ionic-Compound)))

(every CsOH has	
       (has-chemical-name ("caesium hydroxide"))
       (has-part 
	((a Cs-Plus) (a OH-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Cs-Plus)
			 (:pair 1 OH-Minus)))))))
       )(every CsOH has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Cs-Plus) (a OH-Minus))))

