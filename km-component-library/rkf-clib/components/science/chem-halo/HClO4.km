(HClO4 has (superclasses (Ionic-Compound)))

(every HClO4 has	
       (has-chemical-name ("perchloric acid"))
       (has-part 
	((a H-Plus) (a ClO4-Minus)))
       (has-chemical-formula 	
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 H-Plus)
		    (:pair 1 ClO4-Minus))))))))
(every HClO4 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a ClO4-Minus))))

