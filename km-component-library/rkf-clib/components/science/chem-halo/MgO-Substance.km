(MgO-Substance has (superclasses (Ionic-Compound-Substance)))

(every MgO-Substance has 
       (has-basic-structural-unit ((a MgO))))
(every MgO-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a MgO))))
