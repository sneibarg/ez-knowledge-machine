(H2S has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H2S has
	(has-chemical-name ("hydrogen sulfide"))
	(has-part ((a H-Plus)(a S-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 S-Minus-2)))))))
)
(every H2S has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a S-Minus-2))))

