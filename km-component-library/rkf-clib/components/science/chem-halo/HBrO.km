(HBrO has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every HBrO has
	(has-chemical-name ("hypobromous acid"))
	(has-part ((a H-Plus)(a BrO-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 BrO-Minus)))))))
)
(every HBrO has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a BrO-Minus))))

