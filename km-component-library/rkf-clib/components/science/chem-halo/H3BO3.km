(H3BO3 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H3BO3 has
	(has-chemical-name ("boric acid"))
	(has-part ((a H-Plus)(a BO3-Minus-3)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 3 H-Plus)
			(:pair 1 BO3-Minus-3)))))))
)
(every H3BO3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a BO3-Minus-3))))

