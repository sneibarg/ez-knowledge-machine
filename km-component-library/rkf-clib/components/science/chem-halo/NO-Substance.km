(NO-Substance has (superclasses (Inorganic-Molecule-Substance)))
	
(every NO-Substance has
  (has-basic-structural-unit ((a NO))))


(every NO-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a NO))))
