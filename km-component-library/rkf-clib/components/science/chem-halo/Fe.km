(Fe has (superclasses (Metal-Atom VIIIB-Element)))
(every Fe has
       (atomic-symbol (Fe))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 26 NIL))))))
       (has-ion ((a Fe-Plus-3)))
       (chemical-period (4)))

(every Fe has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("iron"))
)
