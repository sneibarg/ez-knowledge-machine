(HClO3 has (superclasses (Ionic-Compound)))

(every HClO3 has	
       (has-chemical-name ("hydrogen chlorate"))
       (has-part 
	((a H-Plus) (a ClO3-Minus)))
       (has-chemical-formula 	
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 H-Plus)
		    (:pair 1 ClO3-Minus))))))))
(every HClO3 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a ClO3-Minus))))

