(O has (superclasses (Non-Metal-Atom VIA-Chalcogen-Element)))
(every O has
	(has-molecular-name ("oxide"))
       (atomic-symbol (O))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 8 NIL))))))
       (has-ion ((a O-Minus-2)))
       (chemical-period (2)))

(every O has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("oxygen"))
)
