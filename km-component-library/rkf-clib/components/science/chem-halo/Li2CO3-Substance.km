(Li2CO3-Substance has (superclasses (Ionic-Compound-Substance)))

(every Li2CO3-Substance has
   (has-basic-structural-unit ((a Li2CO3)))
)

(every Li2CO3-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a Li2CO3))))
