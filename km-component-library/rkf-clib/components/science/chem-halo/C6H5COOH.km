(C6H5COOH has (superclasses (Ionic-Compound)))
(every C6H5COOH has
	(has-chemical-name ("benzoic acid"))
	(has-part ((a H-Plus)(a C7H5O2-Minus)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 1 H-Plus)
			(:pair 1 C7H5O2-Minus)))))))
)
(every C6H5COOH has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a C7H5O2-Minus))))

