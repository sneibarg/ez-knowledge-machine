(NH4NO3 has (superclasses (Salt)))

(every NH4NO3 has
       (has-chemical-name ("ammonium nitrate"))
       (has-part ((a NH4-Plus)
		  (a NO3-Minus)))
       
       (has-chemical-formula ((a Chemical-Formula with
				 (term ((:seq (:pair 1 NH4-Plus)
					      (:pair 1 NO3-Minus))))))
			     )
       )


(every NH4NO3 has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("ammonium nitrate"))
)
