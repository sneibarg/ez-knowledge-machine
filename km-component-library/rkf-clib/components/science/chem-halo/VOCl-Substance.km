(VOCl-Substance has 
  (superclasses (Inorganic-Molecule-Substance)))

(every VOCl-Substance has
  (has-basic-structural-unit (
    (a VOCl)))
)

(every VOCl-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a VOCl))))
