(NaI has (superclasses (Ionic-Compound)))

(every NaI has	
       (has-chemical-name ("sodium iodide"))
       (has-part 
	((a Na-Plus) (a I-Minus)))
       (has-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq (:pair 1 Na-Plus) (:pair 1 I-Minus))))))))

(every NaI has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Na-Plus) (a I-Minus))))

