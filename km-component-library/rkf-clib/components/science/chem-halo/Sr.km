(Sr has (superclasses (Metal-Atom IIA-Alkaline-Earth-Metal-Element)))
(every Sr has
       (atomic-symbol (Sr))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 38 NIL))))))
       (has-ion ((a Sr-Plus-2)))
       (chemical-period (5)))

(every Sr has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("strontium"))
)
