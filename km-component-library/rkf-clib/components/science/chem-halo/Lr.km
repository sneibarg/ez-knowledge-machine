(Lr has (superclasses (Metal-Atom IIIB-Element)))
(every Lr has
       (atomic-symbol (Lr))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 103 NIL))))))
       (chemical-period (7)))
(every Lr has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("lawrencium"))
)
