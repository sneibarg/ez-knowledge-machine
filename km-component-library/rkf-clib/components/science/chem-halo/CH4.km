(CH4 has (superclasses (Hydrocarbon-Molecule)))

(every CH4 has	
       (has-chemical-name ("methane"))
       (nested-atomic-chemical-formula 
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 C)
		    (:pair 4 H))))))))
       


(every CH4 has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("methane"))
)
