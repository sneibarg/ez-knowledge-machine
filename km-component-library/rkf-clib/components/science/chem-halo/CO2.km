(CO2 has (superclasses (Inorganic-Molecule)))

(every CO2 has	
       (has-chemical-name ("carbon dioxide"))
       (nested-atomic-chemical-formula
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 C)
		    (:pair 2 O))))))))
(every CO2 has-definition
       (instance-of (Chemical-Entity))
       (has-chemical-name ("carbon dioxide")))