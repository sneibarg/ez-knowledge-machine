(CH4-Substance has (superclasses (Hydrocarbon-Substance)))
	
(every CH4-Substance has
       (has-chemical-name ("methane"))	
       (has-basic-structural-unit ((a CH4))))
(every CH4-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a CH4))))
