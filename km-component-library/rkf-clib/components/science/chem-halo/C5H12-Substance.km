(C5H12-Substance has (superclasses (Hydrocarbon-Substance)))
	
(every C5H12-Substance has
       (has-chemical-name ("pentane"))	
       (has-basic-structural-unit ((a C5H12))))
(every C5H12-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a C5H12))))
