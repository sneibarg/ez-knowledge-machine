(NH4Cl-Substance has 
  (superclasses (Salt-Substance)))

(every NH4Cl-Substance has
  (has-basic-structural-unit (
    (a NH4Cl)))
)

(every NH4Cl-Substance has-definition
  (instance-of (Salt-Substance))
  (has-basic-structural-unit ((a NH4Cl))))
