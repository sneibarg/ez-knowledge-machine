(Pd has (superclasses (Metal-Atom VIIIB-Element)))
(every Pd has
       (atomic-symbol (Pd))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 46 NIL))))))
       (chemical-period (5)))


(every Pd has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("palladium"))
)
