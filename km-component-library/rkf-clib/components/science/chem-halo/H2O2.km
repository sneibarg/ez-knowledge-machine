(H2O2 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H2O2 has
	(has-chemical-name ("hydrogen peroxide"))
	(has-part ((a H-Plus)(a O2-Minus-2)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 2 H-Plus)
			(:pair 1 O2-Minus-2)))))))
)
(every H2O2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a O2-Minus-2))))

