(C3H8-Substance has (superclasses (Hydrocarbon-Substance)))
	
(every C3H8-Substance has
       (has-chemical-name ("propane"))	
       (has-basic-structural-unit ((a C3H8))))
(every C3H8-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a C3H8))))
