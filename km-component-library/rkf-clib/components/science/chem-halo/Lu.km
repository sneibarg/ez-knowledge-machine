(Lu has (superclasses (Metal-Atom IIIB-Element)))
(every Lu has
       (atomic-symbol (Lu))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 71 NIL))))))
       (chemical-period (6)))
(every Lu has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("lutetium"))
)
