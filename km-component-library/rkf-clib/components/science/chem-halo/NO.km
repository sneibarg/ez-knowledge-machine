(NO has (superclasses (Inorganic-Molecule)))

(every NO has	
       (has-chemical-name ("nitrogen monoxide"))
       (nested-atomic-chemical-formula
	((a Chemical-Formula with
	    (term ((:seq
		    (:pair 1 N)
		    (:pair 1 O))))))))

(every NO has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("nitrogen monoxide"))
)
