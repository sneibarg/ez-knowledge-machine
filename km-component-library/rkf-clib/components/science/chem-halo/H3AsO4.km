(H3AsO4 has (superclasses (Bronsted-Lowry-Acid-Compound)))
(every H3AsO4 has
	(has-chemical-name ("arsenic acid"))
	(has-part ((a H-Plus)(a AsO4-Minus-3)))
	(has-chemical-formula ((a Chemical-Formula with
		(term ((:seq
			(:pair 3 H-Plus)
			(:pair 1 AsO4-Minus-3)))))))
)
(every H3AsO4 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a H-Plus) (a AsO4-Minus-3))))

