;;; Liquid Bromine

(Br2-Substance has (superclasses (Non-Metal)))
	
(every Br2-Substance has
  (has-basic-structural-unit ((a Br2))))
(every Br2-Substance has-definition
  (instance-of (Chemical))
  (has-basic-structural-unit ((a Br2))))
