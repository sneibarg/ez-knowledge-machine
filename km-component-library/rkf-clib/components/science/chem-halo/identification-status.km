(identification-status has
		       (instance-of (CLIB-Slot-Group))
		       (superslots 	())
		       (subslots 	())
		       (domain      (Reaction))    
		       (range       (Identification-Constant))
		       (inverse 	(identification-status-of))
		       (fluent-status (*Inertial-Fluent))
		       ;; (inherit-with-overrides (t))
		       (original-slot      (t))	
		       (cardinality (N-to-1)))

