(Kr has (superclasses (Non-Metal-Atom VIIIA-Noble-Gas-Element)))
(every Kr has
       (atomic-symbol (Kr))
       (atomic-number ((a Atomic-Number-Value with (value ((:pair 36 NIL))))))
       (chemical-period (4)))
(every Kr has-definition

  (instance-of (Chemical-Entity))
  (has-chemical-name ("krypton"))
)
