(C6H5NH2 has (superclasses (Ionic-Compound)))

(every C6H5NH2 has
       (has-chemical-name ("aniline" "phenylamine"))
       (nested-atomic-chemical-formula (
				 (a Chemical-Formula with
				    (term ((:seq 
					    (:pair 6 C)
					    (:pair 5 H)
					    (:pair 1 N)
					    (:pair 2 H)
					    ))))))
       )
#|(every C6H5NH2 has-definition
  (instance-of (Chemical-Entity))
  (has-part ((a Anion) (a Cation))))
|#
