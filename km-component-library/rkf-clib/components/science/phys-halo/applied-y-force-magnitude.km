(applied-y-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (applied-y-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(applied-y-force-magnitude has (wn20-synset ((:set
			       (:triple "applied y force" 0 "n")
			       (:triple "applied vertical force" 0 "n")
			       (:triple "applied vertical force magnitude" 0 "n")
			       (:triple "applied y force magnitude" 0 "n")
			       ))))

(applied-y-force-magnitude-of has (instance-of (EventProperty)))



(applied-y-force-magnitude has (name ("magnitude of the vertical applied force")))
