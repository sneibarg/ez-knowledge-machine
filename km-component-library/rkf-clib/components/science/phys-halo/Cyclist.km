(Cyclist has (superclasses (Role)))

(every Cyclist has
       (played-by ((a Person)))
       (in-event ((a Locomotion with
		     (agent ((the played-by of Self)))
		     (instrument ((a Bicycle)))))))


(Cyclist has (wn20-synset ((:set 
			    (:triple "cyclist" 1 "n")
			    (:triple "bicyclist" 1 "n")
			    (:triple "bicycler" 1 "n")
			    (:triple "wheeler" 4 "n")
			    ))))