(friction-y-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (friction-y-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(friction-y-force-magnitude has (wn20-synset ((:set
			       (:triple "vertical friction force" 0 "n")
			       (:triple "y friction force" 0 "n")
			       (:triple "vertical friction force magnitude" 0 "n")
			       (:triple "y friction force magnitude" 0 "n")
			       ))))

(friction-y-force-magnitude-of has (instance-of (EventProperty)))



(friction-y-force-magnitude has (name ("magnitude of the vertical friction force")))
