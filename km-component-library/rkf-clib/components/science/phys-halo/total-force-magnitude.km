(total-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (total-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(total-force-magnitude has (wn20-synset ((:set
                               (:triple "force" 3 "n")
			       (:triple "total force" 0 "n")
			       (:triple "total force magnitude" 0 "n")
			       ))))

(total-force-magnitude-of has (instance-of (EventProperty)))



(total-force-magnitude has (name ("magnitude of the total force")))
