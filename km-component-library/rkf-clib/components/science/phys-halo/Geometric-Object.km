(Geometric-Object has
  (superclasses (Spatial-Entity))
)

(every Geometric-Object has
  (thickness ((exactly 0 Length-Value)))
)

(Geometric-Object has
  (wn20-synset ((:set (:triple "plane figure" 1 "n")
		      (:triple "two-dimensional figure" 1 "n")
		      (:triple "figure" 6 "n")
		      (:triple "shape" 2 "n")
		      (:triple "form" 6 "n")
))))

