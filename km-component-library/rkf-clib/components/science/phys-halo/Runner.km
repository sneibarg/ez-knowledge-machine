(Runner has (superclasses (Role)))

(every Runner has
       (played-by ((a Person)))
       (in-event ((a Locomotion with
		     (agent ((the played-by of Self)))))))

(Runner has (wn20-synset ((:set 
			   (:triple "runner" 2 "n")
			   ))))