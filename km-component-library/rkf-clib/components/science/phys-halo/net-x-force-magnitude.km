(net-x-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (net-x-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(net-x-force-magnitude has (wn20-synset ((:set
			       (:triple "net x force" 0 "n")
			       (:triple "net horizontal force" 0 "n")
			       (:triple "net x force magnitude" 0 "n")
			       (:triple "net horizontal force magnitude" 0 "n")
			       ))))

(net-x-force-magnitude-of has (instance-of (EventProperty)))


(net-x-force-magnitude has (name ("magnitude of the net horizontal force")))
