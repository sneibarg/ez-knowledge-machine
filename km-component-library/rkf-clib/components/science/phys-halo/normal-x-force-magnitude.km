(normal-x-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (normal-x-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(normal-x-force-magnitude has (wn20-synset ((:set
			       (:triple "normal x force" 0 "n")
			       (:triple "normal horizontal force" 0 "n")
			       (:triple "normal x force magnitude" 0 "n")
			       (:triple "normal horizontal force magnitude" 0 "n")
			       ))))

(normal-x-force-magnitude-of has (instance-of (EventProperty)))



(normal-x-force-magnitude has (name ("magnitude of the horizontal normal force")))
