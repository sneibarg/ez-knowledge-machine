(initial-time has
   (instance-of (EventProperty))
   (superslots 	())
   (subslots 	())
   (domain      (Move))
   (range       (Duration-Value))
   (scalar-constant-class (Time-Constant))
   (cardinal-unit-class   (UoM-Duration))
   (inverse 	(initial-time-of))
   (fluent-status (*Inertial-Fluent))
   ; (inherit-with-overrides (t))
   (cardinality (N-to-1))
)

(initial-time has (wn20-synset ((:set
                               (:triple "initial time" 0 "n")
))))

(initial-time-of has (instance-of (EventProperty)))

(initial-time has (name ("initial time")))
