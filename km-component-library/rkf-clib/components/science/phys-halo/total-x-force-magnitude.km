(total-x-force-magnitude has
  (instance-of (EventProperty))
  (superslots ())
  (subslots ())
  (domain (Exert-Force))
  (range (Force-Magnitude-Value))
  (scale-class (Force-Scale))
  (scalar-constant-class (Force-Constant))
  (cardinal-unit-class (UoM-Force))
  (inverse (total-x-force-magnitude-of))
  (cardinality (N-to-1))
  ; (inherit-with-overrides (t))
  (fluent-status (*Inertial-Fluent))
)

(total-x-force-magnitude has (wn20-synset ((:set
			       (:triple "total x force" 0 "n")
			       (:triple "total horizontal force" 0 "n")
			       (:triple "total x force magnitude" 0 "n")
			       (:triple "total horizontal force magnitude" 0 "n")
			       ))))

(total-x-force-magnitude-of has (instance-of (EventProperty)))



(total-x-force-magnitude has (name ("magnitude of the total horizontal force")))
