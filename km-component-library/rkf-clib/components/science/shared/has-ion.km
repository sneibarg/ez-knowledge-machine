(has-ion has
   (instance-of   	(Participant-Relation))
   (superslots    	())
   (subslots 	  	())
   (domain              (Chemical-Entity))
   (range         	(Ion))
   (inverse 		(is-ion-of))
   (fluent-status 	(*Inertial-Fluent))
   (cardinality 	(1-to-N)))

(is-ion-of has
   (instance-of (Participant-Relation))
   (combine-values-by-appending (t)))


(has-ion has (wn20-synset ((:set
			    (:triple "ion" 0 "n")))))
