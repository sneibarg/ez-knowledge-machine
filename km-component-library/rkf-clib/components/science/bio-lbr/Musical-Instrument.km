(Musical-Instrument has
  (superclasses (Physical-Object))
  (description ("an object used to make musical sounds"))
)

(every Musical-Instrument has
  (instrument-of ((a Create with (result ((a Sound))))))
)

(Musical-Instrument has (wn20-synset ((:set 
                                            (:triple "musical_instrument" 1 "n")
                                            (:triple "instrument" 6 "n")
))))
