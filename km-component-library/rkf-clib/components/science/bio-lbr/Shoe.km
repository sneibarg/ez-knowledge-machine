(Shoe has
  (superclasses (Clothing))
  (description ("covering for the human foot"))
  (cmap-correspondence ())
)

(every Shoe has
  (purpose ((a Instrument-Role with
               (in-event ((a Protect with
                             (object ((a Foot)))
                             (instrument (Self))
               )))
  )))
)

(Shoe has (wn20-synset ((:set
                                  (:triple "shoe" 1 "n")
))))

