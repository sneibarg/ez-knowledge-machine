(Chamber has
  (superclasses (Physical-Object))
  (description ("an enclosed space playing the role of container"))
)

(every Chamber has
  (plays ((a Container)))
)

(Chamber has (wn20-synset ((:set 
                              (:triple "chamber" 1 "n")
                              (:triple "chamber" 2 "n")
))))
