(Lipolysis has
        (superclasses   (Catabolize))
        (required-slot  (object))
        (primary-slot   (agent instrument by-means-of))
	(description	("The catabolic pathway by which lipids are hydrolyzed.
                          Souther.")))

(every Lipolysis has
       (agent ((a Suborganismal-Entity)))
       (object       ((a Lipid)))
       (instrument   ((a Lipase)))
  )




