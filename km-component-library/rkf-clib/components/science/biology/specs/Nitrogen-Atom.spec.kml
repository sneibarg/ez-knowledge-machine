<seealso name=Proton>
<seealso name=Carbon-Atom>
<seealso name=formula>
<seealso name=Oxygen-Atom>
<seealso name=Phosphorus-Atom>
<seealso name=Neutron>
<seealso name=Electron>
<seealso name=Hydrogen-Atom>
<seealso name=Sulfur-Atom>

</component></kml>
