(slot-gloss has
   (instance-of (NL-Slot-Group))
   (superslots ())
   (subslots ())
   (domain (Slot))
   (range (String))
   (fluent-status (*Non-Fluent))
   (original-slot      (t))	
   (cardinality (N-to-1))        ;; go ahead and allow slots to have same gloss
)

(slot-gloss-of has (instance-of (NL-Slot-Group)))
