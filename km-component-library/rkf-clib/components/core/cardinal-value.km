(cardinal-value has
  (instance-of (CLIB-Slot-Group))
  (domain (Property-Value))
  (range (Cardinal))
  (inverse (cardinal-value-of))
  (cardinality (N-to-N))
  (original-slot      (t))	
  (fluent-status (*Inertial-Fluent))
)

(cardinal-value-of has
   (instance-of (CLIB-Slot-Group)))