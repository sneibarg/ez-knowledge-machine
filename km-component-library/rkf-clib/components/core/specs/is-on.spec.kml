<kml>
<relation-doc name=is-on>


<from name=Spatial-Entity>
<to name=Spatial-Entity>

<description> 
	an object or place that is in contact or close proximity and above another
</description>

<examplesentencelist>
   <examplesentence>
	<text><ex-from>The book</ex-from> is lying on <ex-to>the table</ex-to>.</text>
   </examplesentence>

</examplesentencelist>

<neighborlist>
   <neighbor name=is-above>
	is-above does not imply contact or close proximity
   </neighbor>
</neighborlist>

</kml>
