<kml>
<property-doc name=UoM-Length>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description>-- 
  units for measuring linear spatial properties.--(<conceptref name=width>), (<conceptref name=thickness>), (<conceptref name=length>), (<conceptref name=height>), (<conceptref name=distance>), (<conceptref name=depth>)
</description>

<unit-description>
    <unit>*kilometer</unit>
    <unit>*inch</unit>
    <unit>*foot</unit>
    <unit>*yard</unit>
    <unit>*meter</unit>
    <unit>*centimeter</unit>
    <unit>*millimeter</unit>
    <unit>*micrometer</unit>
    <unit>*nanometer</unit></unit-description>

<examplesentencelist>

  <examplesentence>
    <text>25 meter wide pool</text>
    <slotdescription name=property>width</slotdescription>
    <slotdescription name=number>25</slotdescription>
    <slotdescription name=unit of measure>meter</slotdescription>
  </examplesentence>

  <examplesentence>
    <text>He is 6 feet tall</text>
    <slotdescription name=property>height</slotdescription>
    <slotdescription name=number>6</slotdescription>
    <slotdescription name=unit of measure>feet</slotdescription>
  </examplesentence>

<p>
</examplesentencelist>

</kml>



