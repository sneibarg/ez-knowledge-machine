<kml>

<concept name=Carry>
<superconcept name=Move>

<sme-documentation name=Carry>

<subevent name=Locomotion>
<subevent name=Hold>

<hr>
<h3>Component Semantics:</h3>
<component tag=Carry>
  <definition>cause something to change location by holding it while going somewhere</definition>
  <requiredslot name=agent>
    <restriction>holds <slot>object</slot>
    <restriction>"locomotes" (causes self's <slot>location</slot> to change)</restriction>
    <restriction>moves <slot>object</slot> (causes <slot>object</slot>'s <slot>location</slot> to change)</restriction>
  </requiredslot>
  <requiredslot name=object>
    <restriction>a Physical-Object</restriction>
    <restriction>is held by <slot>agent</slot></restriction>
    <restriction>moves (<slot>location</slot> changes)</restriction>
  </requiredslot>
  <primaryslot name=origin>
    <restriction>Place</restriction>
    <restriction>= original <slot>location</slot> of <slot>agent</slot>/<slot>object</slot></restriction>
  </primaryslot>
  <primaryslot name=destination>
    <restriction>Place</restriction>
    <restriction>= final <slot>location</slot> of <slot>agent</slot>/<slot>object</slot></restriction>
  </primaryslot>
  <primaryslot name=path>
    <restriction>Place</restriction>
    <restriction>= transient <slot>location</slot> of <slot>agent</slot>/<slot>object</slot></restriction>
  </primaryslot>
  <wnsynsets><verb>carry#1, carry#2
  </verb></wnsynsets>
  <wnsynsets><verb>pack#2, take#30, transport#1
  </verb></wnsynsets>
  <dd><i>Corresponding Cyc Concept:</i> #$TransportationEvent</dd>
</component>

<hr>
<h3>Core Senses:</h3>
<senselist>
<sense tag=carry1>
  <sense-definition>cause something to change location by holding it while going somewhere</sense-definition>
  <transitivity>transitive</transitivity>
  <requiredslot name=agent>
    <restriction>holds object
    <restriction>causes self's location to change</restriction>
    <restriction>causes object's location to change</restriction>
  </requiredslot>
  <requiredslot name=object>
    <restriction>a Physical-Object</restriction>
    <restriction>is held by agent</restriction>
    <restriction>location changes</restriction>
  </requiredslot>
</sense>
</senselist>

