<kml>
<property-doc name=UoM-Density>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  unit for measuring the <conceptref name=density> of a <conceptref name=Tangible Entity>.
</description>

<unit-description>
    <unit>*gram/liter</unit> 
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>The density of water is 1000 grams per liter.</text>
    <slotdescription name=property>density</slotdescription>
    <slotdescription name=number>1000</slotdescription>
    <slotdescription name=unit of measure>gram/liter</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
