<kml>

<concept name=Condense><system-documentation name=Condense>
<superconcept name=Increase>

<definition>

  An <conceptref name=Increase> in <slot>density</slot>

</definition>

<primaryslotlist>

  <slotdescription name=from-value>
    the initial value of <slot>density</slot> before the event
  </slotdescription>

  <slotdescription name=to-value>
    the final value of <slot>density</slot> after the event
  </slotdescription>

</primaryslotlist>

<examplesentencelist>

  <examplesentence>
    <text>Water condensed on the window.</text>
    <slotdescription name=base>water</slotdescription>
  </examplesentence>

</examplesentencelist>


</kml>


