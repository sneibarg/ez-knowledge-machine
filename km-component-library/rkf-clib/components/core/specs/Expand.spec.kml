<kml>

<concept name=Expand><system-documentation name=Expand>
<superconcept name=Increase>

<definition>

  An <conceptref name=Increase> in <slot>volume</slot>

</definition>

<primaryslotlist>

  <slotdescription name=from-value>
    the initial value of the <slot>volume</slot> before the expansion
  </slotdescription>

  <slotdescription name=to-value>
    the final value of the <slot>volume</slot> after the expansion
  </slotdescription>

</primaryslotlist>

<examplesentencelist>

  <examplesentence>
    <text>The bolt expanded when it got hot.</text>
    <slotdescription name=base>the bolt</slotdescription>
    <slotdescription name=caused-by>heating</slotdescription>
  </examplesentence>

</examplesentencelist>

<neighborlist>

  <neighbor name=Contract>
     To opposite of <conceptref name=Expand>
  </neighbor>

</neighborlist>
</kml>


