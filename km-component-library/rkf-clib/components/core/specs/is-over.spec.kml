<kml>
<relation-doc name=is-over/is-under>


<from name=Spatial-Entity>
<to name=Spatial-Entity>

<description> 
	a place or object higher than or above another 
</description>

<examplesentencelist>
   <examplesentence>
	<text><ex-from>one flew</ex-from> over </ex-to>the cocoo's nest</ex-to>.</text>
   </examplesentence>

   <examplesentence>
	<text><ex-from>The tree</ex-from> grew over <ex-to>the old Roman road</ex-to>.</text>
   </examplesentence>
</examplesentencelist>

<neighborlist>
   <neighbor name=is-above>
	does not imply touching between the two places or objects.
   </neighbor>
</neighborlist>

</kml>
