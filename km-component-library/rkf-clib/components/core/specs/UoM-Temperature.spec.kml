<kml>
<property-doc name=UoM-Temperature>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring the <conceptref name=temperature> of a <conceptref name=Tangible Entity>.
</description>

<unit-description>
    <unit>*fahrenheit</unit>  
    <unit>*celsius</unit>  
    <unit>*kelvin</unit> 
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>Water is most dense at 4 degrees celsius.</text>
    <slotdescription name=property>temperature</slotdescription>
    <slotdescription name=number>4</slotdescription>
    <slotdescription name=unit of measure>celsius</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
