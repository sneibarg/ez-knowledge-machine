<kml>
<property-doc name=UoM-Mass>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring the <conceptref name=mass> of a <conceptref name=Tangible Entity>.
</description>

<unit-description>
    <unit>*gram</unit> 
    <unit>*kilogram</unit> 
    <unit>*dalton</unit> 
    <unit>*slug</unit> 
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>I bought 2 kilograms of chocolate yesterday.</text>
    <slotdescription name=property>mass</slotdescription>
    <slotdescription name=number>2</slotdescription>
    <slotdescription name=unit of measure>kilogram</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
