<kml>

<concept name=Lengthen><system-documentation name=Lengthen>
<superconcept name=Increase>

<definition>

  An <conceptref name=Entity> increases in <slot>length</slot>

</definition>

<requiredslotlist>

  <slotdescription name=object>
     the entity that becomes longer
  </slotdescription>

</requiredslotlist>

<primaryslotlist>

  <slotdescription name=from-value>
    the initial value of the <slot>length</slot> before the event
  </slotdescription>

  <slotdescription name=to-value>
    the final value of the <slot>length</slot> after the event
  </slotdescription>

</primaryslotlist>

<examplesentencelist>

  <examplesentence>
    <text>John lengthened the rope.</text>
    <slotdescription name=agent>John</slotdescription>
    <slotdescription name=object>the rope</slotdescription>
  </examplesentence>

</examplesentencelist>


</kml>


