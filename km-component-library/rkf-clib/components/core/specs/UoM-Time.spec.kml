<kml>
<property-doc name=UoM-Time>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description>-- 
  units for measuring temporal properties.(<conceptref name=duration>), (<conceptref name=age>)
</description>

<unit-description>
    <unit>*day</unit>
    <unit>*month</unit>
    <unit>*year</unit>
    <unit>*hour</unit>
    <unit>*minute</unit></unit-description>

<examplesentencelist>

  <examplesentence>
    <text>12 year old Scotch</text>
    <slotdescription name=property>age</slotdescription>
    <slotdescription name=number>12</slotdescription>
    <slotdescription name=unit of measure>year</slotdescription>
  </examplesentence><p>

  <examplesentence>
    <text>Roger ran for four minutes.</text>
    <slotdescription name=property>duration</slotdescription>
    <slotdescription name=number>4</slotdescription>
    <slotdescription name=unit of measure>minute</slotdescription>
  </examplesentence>

</examplesentencelist>

</kml>


    <slotdescription name=unit of measure>minute</slotdescription>
  </examplesentence>

</examplesentencelist>

</kml>


