    <unit>*weekly</unit> 
    <unit>*monthly</unit> 
    <unit>*yearly</unit> 
    <unit>*per-hour</unit> 
    <unit>*per-minute</unit> 
    <unit>*per-second</unit> 
    <unit>*hertz</unit> 
    <unit>*kilohertz</unit> 
    <unit>*megahertz</unit> 
    <unit>*gigahertz</unit> 
</unit-description>

<examplesentencelist>
