<kml>
<property-doc name=UoM-Angle>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring the <conceptref name=angle> of a <conceptref name=Spatial Entity>.
</description>

<unit-description>
    <unit>*degree</unit> 
    <unit>*radian</unit> 
    <unit>*grad</unit> 
    <unit>*arcminute</unit> 
    <unit>*arcsecond</unit>
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>A right angle is ninety degrees.</text>
    <slotdescription name=property>angle</slotdescription>
    <slotdescription name=number>90</slotdescription>
    <slotdescription name=unit of measure>degree</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
