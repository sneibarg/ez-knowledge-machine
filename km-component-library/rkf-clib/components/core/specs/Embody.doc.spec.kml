<kml>

<concept name=Embody::doc>
<superconcept name=Create::doc>

<sme-documentation name=Embody>

<hr>
<h3>Component Semantics:</h3>
<component tag=Embody>
  <definition>embody a message into a <conceptref name=Entity> which plays a <conceptref name=Signal>
     that can be transmitted</definition>
  <requiredslot name=agent>
    <inheritedfrom name=Create::doc>
    <restriction>a <conceptref name=Entity> which embodies the <conceptref name=Message>
    </restriction>
  </requiredslot>
  <requiredslot name=object>
    <restriction>a <conceptref name=Message> that will be embodied
    </restriction>
  </requiredslot>
  <requiredslot name=result>
    <inheritedfrom name=Create::doc>
    <restriction>a <conceptref name=Entity> which embodies the message
    </restriction>
  </requiredslot>
  <wnsynsets><verb>embody#3
  </verb></wnsynsets>
  <wnsynsets><verb>
  </verb></wnsynsets>
</component>

</kml>