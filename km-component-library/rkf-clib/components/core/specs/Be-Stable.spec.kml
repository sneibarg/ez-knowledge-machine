<kml>

<concept name=Be-Stable><system-documentation name=Be-Stable>

<superconcept name=State>

<definition>

  The state of an entity in which it's Stable, i.e. resistant to changes
  caused by some entity or some action.  

</definition>

<requiredslotlist>

   <slotdescription name=object>
      the entity that's stable
   </slotdescription>

</requiredslotlist>

<primaryslotlist>
   <slotdescription name=base>
      the <slot>object</slot> resists changes caused by
      this entity or action
   </slotdescription>

   <slotdescription name=resulting-from>
      the <conceptref name=Action> that creates this stability
   </slotdescription>

   <slotdescription name=intensity>
      the degree of stability
   </slotdescription>

</primaryslotlist>


<examplesentencelist>

   <examplesentence>
     <text>The goalie was stable on his feet.</text>
     <slotdescription name=object>the goalie</slotdescription>
     <slotdescription name=base><conceptref name=Fall>'ing</slotdescription>
     <slotdescription name=intensity>*strong</slotdescription>     
   </examplesentence>
   
   <examplesentence>
     <text>The tent became unstable when the pole broke.</text>
     <slotdescription name=object>the tent</slotdescription>
     <slotdescription name=resulting-from>breaking of the pole</slotdescription>
     <slotdescription name=intensity>*weak</slotdescription>     
   </examplesentence>

</examplesentencelist>

   
</kml>


