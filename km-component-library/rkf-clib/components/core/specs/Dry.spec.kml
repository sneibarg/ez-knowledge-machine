<kml>

<concept name=Dry><system-documentation name=Dry>
<superconcept name=Decrease>

<definition>

  A <conceptref name=Decrease> in <slot>wetness</slot>

</definition>

<primaryslotlist>

  <slotdescription name=from-value>
    the initial value of <slot>wetness</slot> before the drying
  </slotdescription>

  <slotdescription name=to-value>
    the final value of <slot>wetness</slot> after the drying
  </slotdescription>

</primaryslotlist>

<examplesentencelist>

  <examplesentence>
    <text>The clothes dryed from soaking wet to damp.</text>
    <slotdescription name=base>the clothes</slotdescription>
    <slotdescription name=from-value>soaking wet (with respect to cloth)</slotdescription>
    <slotdescription name=to-value>damp (with respect to cloth)</slotdescription>
  </examplesentence>

</examplesentencelist>
<neighborlist>

  <neighbor name=Wet>
     To opposite of <conceptref name=Dry>
  </neighbor>

</neighborlist>

</kml>


