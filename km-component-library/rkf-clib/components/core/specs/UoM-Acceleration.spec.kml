<kml>
<property-doc name=UoM-Acceleration>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring <conceptref name=acceleration>.
</description>

<unit-description>
    <unit>*centimeter-per-second-squared </unit>
    <unit>*foot-per-second-squared</unit>
    <unit>*meter-per-second-squared</unit>
    <unit>*kilometer-per-second-squared</unit>
    <unit>*mile-per-second-squared</unit>
    <unit>*kilometer-per-hour-minute</unit>
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>The car's speed was going up at 10 meter-per-second-squared.</text>
    <slotdescription name=property>acceleration</slotdescription>
    <slotdescription name=number>10</slotdescription>
    <slotdescription name=unit of measure>meter-per-second-squared</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
