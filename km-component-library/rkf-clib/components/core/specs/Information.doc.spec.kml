<kml>

<concept name=Information>
<superconcept name=Intangible-Entity>

<hr>
<hr3>Component Semantics:</h3>
<component tag=Information>
	<definition>Knowledge communicated about a particular fact or circumstance.</definition>

<requiredslot name=information-content>
	<restriction>a Thing (a frame, a triple, basically anything)</restriction>
</requiredslot>

<primaryslot name=truth>
	<restriction>a Truth-Value</restriction>
</primaryslot>

  <wnsynsets><noun>information#1
  </noun></wnsynsets>
  <wnsynsets><noun>info#1
  </noun></wnsynsets>

</component>
</kml>
