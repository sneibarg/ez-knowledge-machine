<kml>
<relation-doc name=object>


<from name=Event>
<to name=Entity>

<description> 
  the entity that is acted upon by an Event.  The main passive
  participant in the Event.
</description>

<examplesentencelist>

  <examplesentence>
    <text>John <ex-from>swatted</ex-from> <ex-to>the fly</ex-to>.</text>
  </examplesentence>

  <examplesentence>
    <text>The robber <ex-from>cracked open</ex-from> <ex-to>the safe</ex-to>.</text>
  </examplesentence>

  <examplesentence>
    <text>The bat <ex-from>struck</ex-from> <ex-to>the ball</ex-to> forcefully.</text>
  </examplesentence>

</examplesentencelist>

</kml>
