<kml>
<relation-doc name=has-example>

<from name=Thing>
<to name=Thing>

<description>
  Something is a typical example of a Thing.
</description>

<examplesentencelist>

  <examplesentence>
    <text><ex-to>John</ex-to> is an example of a  <ex-from>Person</ex-from>.</text>
  </examplesentence>

  <examplesentence>
    <text><ex-to>HCl</ex-to> is an example of an <ex-from>acid</ex-from>.</text>
  </examplesentence>

</examplesentencelist>

</kml>

