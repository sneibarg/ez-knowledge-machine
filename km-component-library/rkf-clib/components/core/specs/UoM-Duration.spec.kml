<kml>
<property-doc name=UoM-Duration>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring the <conceptref name=duration> of an <conceptref name=Event> or a <conceptref name=Time-Interval>.
</description>

<unit-description>
    <unit>*day</unit> 
    <unit>*week</unit> 
    <unit>*month</unit> 
    <unit>*year</unit> 
    <unit>*hour</unit> 
    <unit>*minute</unit> 
    <unit>*second</unit> 
    <unit>*millisecond</unit> 
    <unit>*microsecond</unit> 
    <unit>*nanosecond</unit> 
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>The second car overtook the first one in precisely 7 seconds.</text>
    <slotdescription name=property>duration</slotdescription>
    <slotdescription name=number>7</slotdescription>
    <slotdescription name=unit of measure>second</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
