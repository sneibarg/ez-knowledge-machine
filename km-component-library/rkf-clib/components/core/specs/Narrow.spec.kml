<kml>

<concept name=Narrow><system-documentation name=Narrow>
<superconcept name=Decrease>

<definition>

  An <conceptref name=Entity> decreases in <slot>width</slot>

</definition>

<requiredslotlist>

  <slotdescription name=object>
     the entity that becomes more narrow
  </slotdescription>

</requiredslotlist>

<primaryslotlist>

  <slotdescription name=from-value>
    the initial value of the <slot>width</slot> before the event
  </slotdescription>

  <slotdescription name=to-value>
    the final value of the <slot>width</slot> after the event
  </slotdescription>

</primaryslotlist>

<examplesentencelist>

  <examplesentence>
    <text>John made the board narrow with the plane.</text>
    <slotdescription name=agent>John</slotdescription>
    <slotdescription name=object>the board</slotdescription>
    <slotdescription name=instrument>plane</slotdescription>
  </examplesentence>

</examplesentencelist>


</kml>


