<kml>
<property-doc name=UoM-Volume>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  units for measuring the <conceptref name=volume> of a <conceptref name=Spatial Entity>.
</description>

<unit-description>
    <unit>*milliliter</unit> 
    <unit> *liter</unit>  
    <unit>*gallon</unit>
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>My family easily finishes a liter of milk each day.</text>
    <slotdescription name=property>volume</slotdescription>
    <slotdescription name=number>1</slotdescription>
    <slotdescription name=unit of measure>liter</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
