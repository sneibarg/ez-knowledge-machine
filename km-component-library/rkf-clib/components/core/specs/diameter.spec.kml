<kml>
<property-doc name=diameter>
<gpd-documentation name=Property>

<from name=Spatial-Entity>
<to name=Length-Value>

<description> 
    the length of a straight line through the center of a Spatial-Entity
</description>

<value-description>
  <cardinal-value-description>
    a number along with any of the units of measurement for <conceptref name=UoM-Length>
<p>
    <examplesentence>
      <text>2 meter</text>
      <slotdescription name=number>2</slotdescription>
      <slotdescription name=unit of measure>meter</slotdescription>
    </examplesentence>
  </cardinal-value-description>

</value-description>
</kml>

