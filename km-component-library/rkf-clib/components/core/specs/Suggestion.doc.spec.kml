<kml>

<concept name=Suggestion>
<superconcept name=Information>

<hr>
<h3>Component Semantics:</h3>
<component tag=Suggestion>
  <definition>A message (an <conceptref name =Information>) suggesting something.</definition>

  <primaryslot name=information-content>
    <inheritedfrom name=Information>
    <restriction>a Thing</restriction>
  </primaryslot>
  
  <wnsynsets>
  <noun>suggestion#1,suggesting#2,proposition#2,proffer#1
  </noun>
  </wnsynsets>
</component>

<musing>

</musing>

