<kml>
<property-doc name=UoM-Brightness>
<superconcept name=Unit-of-Measurement>
<gpd-documentation name=Property>

<description> 
  unit for measuring the <conceptref name=brightness> of a <conceptref name=Tangible Entity>.
</description>

<unit-description>
    <unit>*candela</unit> 
</unit-description>

<examplesentencelist>

  <examplesentence>
    <text>The light bulb had a brightness of 10 candela.</text>
    <slotdescription name=property>brightness</slotdescription>
    <slotdescription name=number>10</slotdescription>
    <slotdescription name=unit of measure>candela</slotdescription>
  </examplesentence>
</examplesentencelist>

</kml>
