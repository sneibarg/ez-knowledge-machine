<kml>

<property-doc name=direction>
<system-documentation name=Angle-Value>
<gpd-documentation name=Property>

<from name=Event>
<from name=Property-Vector-Value>
<to name=Angle-Value>

<description>
  the angle of direction of an Event
</description>

<value-description>
  <cardinal-value-description>
    a number along with any of the units of measurement for <conceptref name=UoM-Angle>
<p>
    <examplesentence>
      <text>90 degrees</text>
      <slotdescription name=number>90</slotdescription>
      <slotdescription name=unit of measure>*degree</slotdescription>
    </examplesentence>
  </cardinal-value-description>
</value-description>
</kml>
