(Number-Field has
  (superclasses (Message-Field))
  (description ("a message field whose content is a numeric constant"))
)

(every Number-Field has-definition
  (instance-of (Message-Field))
  (information-content ((a Number)))
)

(every Number-Field has
  (information-content ((a Number)))
)

