(categorical-constant has
  (instance-of (CLIB-Slot-Group))
  (domain (Categorical))
  (range (Constant))
  (inverse (categorical-constant-of))
  (cardinality (N-to-1))
  (original-slot      (t))	
  (fluent-status (*Inertial-Fluent))
)

(categorical-constant-of has
   (instance-of (CLIB-Slot-Group)))
