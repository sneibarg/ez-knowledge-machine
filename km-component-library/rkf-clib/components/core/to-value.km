(to-value has
  (instance-of (Property-Relation))
  (domain (Increase Decrease))
  (range (Property-Value))
  (inverse (to-value-of))
  (cardinality (N-to-1))
  (fluent-status (*Inertial-Fluent))
  (inherit-with-overrides (t))
  (original-slot      (t))	
)

(to-value-of has
	(instance-of (Property-Relation))
)
