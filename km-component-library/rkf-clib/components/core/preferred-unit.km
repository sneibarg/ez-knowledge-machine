(preferred-unit has
   (instance-of (CLIB-Slot-Group))
   (domain (Property-Value))
   (range (Unit-of-Measurement))
   (inverse (preferred-unit-of))
   (fluent-status (*Inertial-Fluent))
   (inherit-with-overrides (t))
   (original-slot      (t))	
   (cardinality (N-to-1))
)

(preferred-unit-of has
   (instance-of (CLIB-Slot-Group)))