(Text-Field has
  (superclasses (Message-Field))
  (description ("a message field whose content is a text constant"))
)

(every Text-Field has-definition
  (instance-of (Message-Field))
  (information-content ((a String)))
)

(every Text-Field has
  (information-content ((a String)))
)

