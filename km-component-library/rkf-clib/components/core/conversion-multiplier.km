(conversion-multiplier has
  (instance-of (CLIB-Slot-Group))
  (domain (Unit-of-Measurement))
  (range (Number))
  (inverse (conversion-multiplier-of))
  (cardinality (N-to-1))
  (fluent-status (*Inertial-Fluent))
)

(conversion-multiplier-of has
   (instance-of (CLIB-Slot-Group))
)
