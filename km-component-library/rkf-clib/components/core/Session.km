(Session has
	    (superclasses (Interaction))      
	    (required-slot          ()) 
	    (primary-slot     	    ())  
	    (secondary-slot         ())
)

(Session has 
	     (wn20-synset 
	      ((:set 
		(:triple "session" 1 "n")
		(:triple "session" 3 "n")))))

(every Session has (text-nominalization ("session")))