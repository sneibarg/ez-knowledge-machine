(plays has
       (instance-of (Participant-Relation))
       (subslots ())
       (domain (Entity))
       (range (Role))
       (inverse (played-by))
       (cardinality (1-to-N))
       (original-slot      (t))	
       (fluent-status (*Inertial-Fluent)))

(plays has (text-gloss ("plays the role of")))
(played-by has 
	(instance-of (Participant-Relation))
	(text-gloss ("is the role played by")))
