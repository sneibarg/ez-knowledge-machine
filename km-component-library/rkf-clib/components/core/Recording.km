(Recording has
	(description ("A kind of Message created by a Sensor as the result of a Record event."))
	(superclasses (Message)))

(every Recording has
	(result-of ((a Record)))) 
	
(Recording has (wn20-synset ((:set
                           (:triple "recording" 1 "n")

))))


