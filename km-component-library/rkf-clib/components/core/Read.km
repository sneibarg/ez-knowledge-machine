(Read has (description ("The act of reading a physical written document."))
	  (superclasses (Sense))
        #|(sentence-type (*punctual *conclusive))|#
)

(every Read has 
	(object ((a Physical-Document)(must-be-a Physical-Document))))

(Read has (wn20-synset ((:set
			   (:triple "read" 1 "v")
))))
	  
(every Read has (text-nominalization ("reading")))