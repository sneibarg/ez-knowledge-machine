(Hear has 
	(description ("Sensing a sound auditorily.")) 
	(superclasses (Sense))
      #|(sentence-type (*punctual *conclusive))|#
)

(every Hear has
	(object ((a Sound)(must-be-a Sound))))


(Hear has (wn20-synset ((:set
			   (:triple "hear" 1 "v")
))))


(every Hear has (text-nominalization ("hearing")))