(in-event has
	  (instance-of (Participant-Relation))
	  (superslots ())
	  (subslots ())
	  (domain (Role))
	  (range (Event))
	  (inverse (in-event-of))
	  (cardinality (N-to-1))
  (original-slot      (t))	
	  (fluent-status (*Inertial-Fluent)))

(in-event-of has
	(instance-of (Participant-Relation)))
