(Teacher has
	(superclasses (Role)))

(every Teacher has
	(in-event ((must-be-a Teach)))
	(played-by ((a Tangible-Entity with (agent-of ((the in-event of Self))))))
)

(Teacher has 
       (wn20-synset 
	((:set (:triple "teacher" 1 "n"))
	 (:set (:triple "instructor" 1 "n")))))
