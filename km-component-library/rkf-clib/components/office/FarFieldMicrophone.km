(FarFieldMicrophone has
	(description ("A type of Microphone which usually remains stationary."))
	(superclasses (Microphone)))
(FarFieldMicrophone has
  (feature-slot ((the feature-slot of Microphone)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = FarFieldMicrophone)))))

