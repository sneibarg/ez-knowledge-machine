(Discussant has 
  (superclasses (Participant))
  (description ("a role played by a Participant in a Discussion"))
)

(every Discussant has
  (in-event ((a Discussion)))
)

(Discussant has (wn20-synset ((:set
	 		     (:triple "discussant" 1 "n")
))))
