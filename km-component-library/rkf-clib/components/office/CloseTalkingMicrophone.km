(CloseTalkingMicrophone has
	(description ("A type of Microphone which can be worn a Person and thus moves around with them."))
	(superclasses (Microphone)))

(CloseTalkingMicrophone has
  (feature-slot ((the feature-slot of Microphone)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = CloseTalkingMicrophone)))))

