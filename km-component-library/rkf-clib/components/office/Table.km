(Table has (superclasses (Furniture)))

(every Table has
       (purpose ((a Instrument-Role with
		    (in-event ((must-be-a Support)))
		    (played-by (Self)))))
       )

(Table has (wn20-synset ((:set
			     (:triple "table" 2 "n")
			     (:triple "table" 3 "n")
))))