(ComputerCopyMachine has 
  (superclasses (ComputerPeripheral Photocopier)))

(ComputerCopyMachine has
  (feature-slot ((the feature-slot of ComputerPeripheral) (the feature-slot of Photocopier)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = ComputerCopyMachine)))))

