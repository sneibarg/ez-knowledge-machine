(Laptop-Accessory has 
  (superclasses (Computer-Accessory))
  (feature-slot ((the feature-slot of Computer-Accessory)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Laptop-Accessory))
  ))
)


