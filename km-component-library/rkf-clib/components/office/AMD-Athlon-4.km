(AMD-Athlon-4 has 
  (superclasses (AMD-Athlon-Processor))
  (feature-slot ((the feature-slot of AMD-Athlon-Processor)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = AMD-Athlon-4))
  ))
)


