(CD-Drive has 
  (superclasses (Optical-Drive))
  (feature-slot ((the feature-slot of Optical-Drive)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = CD-Drive))
  ))
)

(CD-Drive has (wn20-synset ((:set
			     (:triple "CD_drive" 1 "n")
))))

