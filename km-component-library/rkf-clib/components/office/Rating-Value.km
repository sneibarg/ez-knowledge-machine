(Rating-Value has 
  (superclasses (Property-Value)))

(every Rating-Value has
   (property-slot (rating))
  (rating-of ((must-be-a Review)))
  (less-than ((must-be-a Rating-Value)))
  (greater-than ((must-be-a Rating-Value)))
  (same-as ((must-be-a Rating-Value)))
)

