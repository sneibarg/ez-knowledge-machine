(ComputerNetworkPeripheral has 
  (superclasses (Computer-Equipment)))


(ComputerNetworkPeripheral has
  (feature-slot ((the feature-slot of Computer-Equipment)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = ComputerNetworkPeripheral)))))

