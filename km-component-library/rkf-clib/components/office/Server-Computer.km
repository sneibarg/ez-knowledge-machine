(Server-Computer has 
  (superclasses (Computer))
  (feature-slot ((the feature-slot of Computer)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Server-Computer))
  ))
)


(Server-Computer has
		 (wn20-synset ((:set (:triple "server" 3 "n")
				     (:triple "host" 10 "n")))))