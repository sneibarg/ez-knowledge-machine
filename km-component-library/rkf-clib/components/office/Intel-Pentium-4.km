(Intel-Pentium-4 has 
  (superclasses (Intel-Pentium-Processor))
  (feature-slot ((the feature-slot of Intel-Pentium-Processor)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Intel-Pentium-4))
  ))
)


