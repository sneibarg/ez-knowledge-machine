(every Record has
       (instrument ((a Sensor)))
       )

(hasSampleRate has (instance-of (Interface-Slot))(domain (Record))(range (Frequency-Value))(inverse (isSampleRateOf)))

(hasTrackedEntity has (instance-of (Interface-Slot))(domain (Record))(range (Entity))(inverse (isTrackedEntityOf)))

(hasTrackedProperty has (instance-of (Interface-Slot))(domain (Record))(range (Tangible-Entity))(inverse (isTrackedPropertyOf)))




