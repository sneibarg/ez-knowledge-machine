(WirelessNetworkHub has 
  (superclasses (ComputerNetworkPeripheral)))

(WirelessNetworkHub has
  (feature-slot ((the feature-slot of ComputerNetworkPeripheral)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = WirelessNetworkHub)))))

