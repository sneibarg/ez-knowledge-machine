(Envelope has (superclasses (Artifact)))

(every Envelope has
       (purpose ((a Instrument-Role with
                    (in-event  ((must-be-a Carry)))
                    (played-by (Self))
       )))
)

(Envelope has (wn20-synset ((:set
			     (:triple "envelope" 1 "n")
			     (:triple "envelope" 2 "n")
))))