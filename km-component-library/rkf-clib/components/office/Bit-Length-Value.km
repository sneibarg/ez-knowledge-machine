(Bit-Length-Value has 
   (superclasses (Property-Value)))

(every Bit-Length-Value has
   (property-slot (bit-length))
   (data-transfer-rate-of ((must-be-a Computer-Equipment)))
   (less-than ((must-be-a Bit-Length-Value)))
   (greater-than ((must-be-a Bit-Length-Value)))
   (same-as ((must-be-a Bit-Length-Value)))
)

