(Intel-Xeon-MP has 
  (superclasses (Intel-Xeon-Processor))
  (feature-slot ((the feature-slot of Intel-Xeon-Processor)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Intel-Xeon-MP))
  ))
)


