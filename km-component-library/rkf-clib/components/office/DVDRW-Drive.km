(DVDRW-Drive has 
  (superclasses (DVD-Drive))
  (feature-slot ((the feature-slot of DVD-Drive)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = DVDRW-Drive))
  ))
)

