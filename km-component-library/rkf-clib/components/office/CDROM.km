(CDROM has 
  (superclasses (Compact-Disc))
  (feature-slot ((the feature-slot of Compact-Disc)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = CDROM))
  ))
)

(CDROM has (wn20-synset ((:set
			  (:triple "CD-ROM" 1 "n")
			  (:triple "compact_disc_read-only_memory" 1 "n")
))))
