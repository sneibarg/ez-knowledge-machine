(Manufacturer has (superclasses (Organization-Role)))

(every Manufacturer has 
   (in-event ((a Produce with
                 (agent ((the Organization played-by of Self))))))
)

(Manufacturer has (wn20-synset ((:set
	 		     (:triple "manufacturer" 1 "n")
	 		     (:triple "maker" 3 "n")
	 		     (:triple "manufacturing_business" 1 "n")
	 		     (:triple "manufacturer" 2 "n")
	 		     (:triple "producer" 1 "n")
))))

