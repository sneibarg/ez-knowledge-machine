(Letter has (superclasses (Document)))

(every Letter has
       (purpose ((a Medium with
                    (in-event  ((must-be-a Transmit )))
                    (played-by (Self))
       )))
)

(Letter has (wn20-synset ((:set
			     (:triple "letter" 1 "n")
			     (:triple "missive" 1 "n")
))))