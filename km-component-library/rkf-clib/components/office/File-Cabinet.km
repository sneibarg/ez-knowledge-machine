(File-Cabinet has (superclasses (Furniture)))

(every File-Cabinet has
       (purpose ((a Container with
                    (in-event  ((must-be-a Be-Contained)))
		    (content ((a Document)))
                    (played-by (Self))
		    )))
       )

(File-Cabinet has (wn20-synset ((:set
				 (:triple "file_cabinet" 1 "n")
				 (:triple "filing_cabinet" 1 "n")
				 (:triple "file" 3 "n")
))))
