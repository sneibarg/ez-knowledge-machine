(LCD-Projector has 
  (superclasses (Projector))
  (feature-slot ((the feature-slot of Projector)))
  (feature-slot ((allof (the all-instances of Interface-Slot) 
                  where ((the domain of It) = LCD-Projector))
  ))
)


