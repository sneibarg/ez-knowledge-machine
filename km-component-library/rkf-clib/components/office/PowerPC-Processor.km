(PowerPC-Processor has 
  (superclasses (Processor))
  (feature-slot ((the feature-slot of Processor)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = PowerPC-Processor))
  ))
)

(every PowerPC-Processor has
  (result-of ((a Produce with (agent (*IBM-Corporation)))))
)

