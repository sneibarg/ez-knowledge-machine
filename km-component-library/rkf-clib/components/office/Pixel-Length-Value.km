(Pixel-Length-Value has 
   (superclasses (Property-Value)))

(every Pixel-Length-Value has
   (property-slot (pixel-length))
   (pixel-length-of ((must-be-a Computer-Display)))
   (less-than ((must-be-a Pixel-Length-Value)))
   (greater-than ((must-be-a Pixel-Length-Value)))
   (same-as ((must-be-a Pixel-Length-Value)))
)

