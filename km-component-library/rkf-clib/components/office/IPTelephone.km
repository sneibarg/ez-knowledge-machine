(IPTelephone has 
  (superclasses (Telephone ComputerPeripheral)))

(IPTelephone has
  (feature-slot ((the feature-slot of Telephone) (the feature-slot of ComputerPeripheral)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = IPTelephone)))))

