(WallDisplayMonitor has 
  (superclasses (Computer-Display)))

(WallDisplayMonitor has
  (feature-slot ((the feature-slot of Computer-Display)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = WallDisplayMonitor)))))

