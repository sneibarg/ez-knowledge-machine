(Laptop-Case has 
  (superclasses (Laptop-Accessory))
  (feature-slot ((the feature-slot of Laptop-Accessory)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Laptop-Case))
  ))
)



