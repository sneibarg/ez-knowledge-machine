(Flatbed-Scanner has 
  (superclasses (Scanner))
  (feature-slot ((the feature-slot of Scanner)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Flatbed-Scanner))
  ))
)

