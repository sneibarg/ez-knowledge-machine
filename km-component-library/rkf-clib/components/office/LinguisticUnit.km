(LinguisticUnit has
	(description ("A kind of Message that is a recursively defined generic container for linguistic units of all kinds.  A LinguisticUnit may be composed of other LinguisticsUnits through composition."))
	(superclasses (Message)))

(LinguisticUnit has (wn20-synset ((:set
				   (:triple "linguistic_unit" 1 "n")
				   (:triple "language_unit" 1 "n")
))))


