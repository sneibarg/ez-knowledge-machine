(Handheld-Computer has 
  (superclasses (Computer))
  (feature-slot ((the feature-slot of Computer)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Handheld-Computer))
  ))
)


(Handheld-Computer has
		   (wn20-synset ((:set
				  (:triple "hand-held_computer" 1 "n")
				  (:triple "hand-held_microcomputer" 1 "n")
))))
