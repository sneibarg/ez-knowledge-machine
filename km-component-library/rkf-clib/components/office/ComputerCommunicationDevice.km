(ComputerCommunicationDevice has 
  (superclasses (ComputerPeripheral)))

(ComputerCommunicationDevice has
  (feature-slot ((the feature-slot of ComputerPeripheral)))
  (feature-slot ((allof (the all-instances of Interface-Slot) where ((the domain of It) = ComputerCommunicationDevice)))))

