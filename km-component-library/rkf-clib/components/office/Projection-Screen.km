(Projection-Screen has (superclasses (Artifact)))

(every Projection-Screen has 
       (purpose ((a Medium with
       		    (in-event  ((must-be-a Display)))
       		    (played-by (Self))
       )))
)

(Projection-Screen has (wn20-synset ((:set
  		       (:triple "screen" 1 "n")
  		       (:triple "silver_screen" 2 "n")
  		       (:triple "projection_screen" 1 "n")
))))