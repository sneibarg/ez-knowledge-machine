(Compact-Disc has 
  (superclasses (Computer-Storage))
  (feature-slot ((the feature-slot of Computer-Storage)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Compact-Disc))
  ))
)

(Compact-Disc has (wn20-synset ((:set
				 (:triple "compact_disc" 1 "n")
				 (:triple "compact_disk" 1 "n")
				 (:triple "CD" 4 "n")
))))
