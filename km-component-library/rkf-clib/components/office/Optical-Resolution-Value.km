(Optical-Resolution-Value has 
   (superclasses (Property-Value)))

(every Optical-Resolution-Value has
   (property-slot (optical-resolution))
   (less-than ((must-be-a Optical-Resolution-Value)))
   (greater-than ((must-be-a Optical-Resolution-Value)))
   (same-as ((must-be-a Optical-Resolution-Value)))
)

