(Flat-Panel-Display has 
  (superclasses (Computer-Display))
  (feature-slot ((the feature-slot of Computer-Display)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Flat-Panel-Display))
  ))
)

