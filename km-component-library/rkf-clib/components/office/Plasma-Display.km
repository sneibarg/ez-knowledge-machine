(Plasma-Display has 
  (superclasses (Flat-Panel-Display))
  (feature-slot ((the feature-slot of Flat-Panel-Display)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Plasma-Display))
  ))
)

