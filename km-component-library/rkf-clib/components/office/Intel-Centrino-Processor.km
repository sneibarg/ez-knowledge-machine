(Intel-Centrino-Processor has 
  (superclasses (Intel-Processor))
  (feature-slot ((the feature-slot of Intel-Processor)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Intel-Centrino-Processor))
  ))
)


