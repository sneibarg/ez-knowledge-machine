(Apple-Macintosh has 
  (superclasses (Computer))
  (feature-slot ((the feature-slot of Computer)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Apple-Macintosh))
  ))
)

