(Note-Book has (superclasses (Artifact)))

(every Note-Book has
       (purpose ((a Container with
		    (content ((a Message)))
		    (in-event ((must-be-a Be-Contained)))
		    (played-by (Self)))))
       )

(Note-Book has (wn20-synset ((:set
  		       (:triple "notebook" 1 "n")
))))