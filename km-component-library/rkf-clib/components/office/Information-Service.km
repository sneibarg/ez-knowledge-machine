; IRIS_class: InformationService

;; a Service offering information
(Information-Service has (superclasses (Service)))

(every Information-Service has
       (object ((a Information)))
       (subevent ((a Communicate with
		     (donor ((the donor of Self)))
		     (recipient ((the recipient of Self)))
		     (object ((the object of Self)))))))

