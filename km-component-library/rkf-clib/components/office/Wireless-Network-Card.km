(Wireless-Network-Card has 
  (superclasses (Network-Card))
  (feature-slot ((the feature-slot of Network-Card)))
  (feature-slot ((allof (the all-instances of Interface-Slot)
                  where ((the domain of It) = Wireless-Network-Card))
  ))
)

