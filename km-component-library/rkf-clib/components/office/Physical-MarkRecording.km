(Physical-MarkRecording has
	(description ("A type of Recording produced when the object of the recording constitutes physical markings (i.e. is an instance of Phsical-Mark) such as writing, sketching, etc."))
	(superclasses (Recording)))

(every Physical-MarkRecording has
	(result-of ((a Record with (object ((a Physical-Mark)))))))
